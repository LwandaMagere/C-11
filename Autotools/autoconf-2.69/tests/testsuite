# We use aclocal (via autoreconf).
{ set +x
$as_echo "$at_srcdir/torture.at:1702: aclocal --version || exit 77"
at_fn_check_prepare_trace "torture.at:1702"
( $at_check_trace; aclocal --version || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1702"
$at_failed && at_fn_log_failure
$at_traceon; }


: >install-sh
cat >configure.in <<'_ATEOF'
AC_INIT(GNU Outer, 1.0)
AC_CONFIG_SUBDIRS([inner])
AC_OUTPUT
_ATEOF


as_dir=inner; as_fn_mkdir_p

cat >inner/configure <<'_ATEOF'
#! /bin/sh
case "$*" in
    *--help*) echo 'No Autoconf here, folks!' ;;
    *)        echo got_it >myfile ;;
esac
exit 0
_ATEOF

chmod +x inner/configure

{ set +x
$as_echo "$at_srcdir/torture.at:1723: autoreconf -Wall -v"
at_fn_check_prepare_trace "torture.at:1723"
( $at_check_trace; autoreconf -Wall -v
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1723"
$at_failed && at_fn_log_failure
$at_traceon; }


# Running the outer configure recursively should provide the innermost
# help strings.
{ set +x
$as_echo "$at_srcdir/torture.at:1727: ./configure \$configure_options --help=recursive | grep \"folks\""
at_fn_check_prepare_notrace 'a shell pipeline' "torture.at:1727"
( $at_check_trace; ./configure $configure_options --help=recursive | grep "folks"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1727"
$at_failed && at_fn_log_failure
$at_traceon; }


# Running the outer configure should trigger the inner.
{ set +x
$as_echo "$at_srcdir/torture.at:1730: ./configure \$configure_options"
at_fn_check_prepare_dynamic "./configure $configure_options" "torture.at:1730"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1730"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:1731: test -f inner/myfile"
at_fn_check_prepare_trace "torture.at:1731"
( $at_check_trace; test -f inner/myfile
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1731"
$at_failed && at_fn_log_failure
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
#AT_STOP_257
#AT_START_258
at_fn_group_banner 258 'torture.at:1740' \
  "Non-literal AC_CONFIG_SUBDIRS" "                  " 7
at_xfail=no
(
  $as_echo "258. $at_setup_line: testing $at_desc ..."
  $at_traceon



: >install-sh
cat >configure.in <<'_ATEOF'
AC_INIT(GNU Outer, 1.0)

my_subdirs=
# Taken from autoconf.texi:Subdirectories.
if test "x$package_foo_enabled" = xyes; then
  my_subdirs="$my_subdirs foo"
fi
AC_CONFIG_SUBDIRS([$my_subdirs])
AC_OUTPUT
_ATEOF


# Hand-written aclocal.m4, so we don't invoke `aclocal -Werror' which
# could barf over warnings in third-party macro files, or fail over
# warnings with older aclocal which didn't yet understand -W*.
: >aclocal.m4

as_dir=foo; as_fn_mkdir_p

cat >foo/configure <<'_ATEOF'
#! /bin/sh
touch innerfile
exit 0
_ATEOF

chmod +x foo/configure

# autoreconf should warn without -Wno-syntax, but should not fail without -Werror.
{ set +x
$as_echo "$at_srcdir/torture.at:1771: autoreconf -Werror -v"
at_fn_check_prepare_trace "torture.at:1771"
( $at_check_trace; autoreconf -Werror -v
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:1771"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
