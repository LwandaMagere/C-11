$as_echo "$at_srcdir/c.at:75: autoheader "
at_fn_check_prepare_trace "c.at:75"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/c.at:75"
$at_failed && at_fn_log_failure
$at_traceon; }



for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/c.at:75: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "c.at:75"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
