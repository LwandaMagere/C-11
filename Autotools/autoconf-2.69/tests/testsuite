cat >fake-shell <<'_ATEOF'
#!/bin/sh
echo 'Fake shell executed.'
shift # fake shell
echo "nargs = $#"
for i
do
  printf ' :%s:\n' "$i"
done
_ATEOF

chmod a+x fake-shell

