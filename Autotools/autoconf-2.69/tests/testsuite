{ set +x
$as_echo "$at_srcdir/torture.at:1292: autoconf --force "
at_fn_check_prepare_trace "torture.at:1292"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1292"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "torture.at:1292" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/torture.at:1292"
{ set +x
$as_echo "$at_srcdir/torture.at:1292: /bin/sh -n configure"
at_fn_check_prepare_trace "torture.at:1292"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1292"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

 { set +x
$as_echo "$at_srcdir/torture.at:1293: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "torture.at:1293"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:1293"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


