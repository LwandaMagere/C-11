$as_echo "$at_srcdir/torture.at:1273: cd space && { \$MAKE || exit 77; }"
at_fn_check_prepare_dynamic "cd space && { $MAKE || exit 77; }" "torture.at:1273"
( $at_check_trace; cd space && { $MAKE || exit 77; }
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1273"
$at_failed && at_fn_log_failure
$at_traceon; }

