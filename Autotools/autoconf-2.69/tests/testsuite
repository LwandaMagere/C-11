#AT_STOP_236
#AT_START_237
at_fn_group_banner 237 'torture.at:186' \
  "AC_CONFIG_FILES, HEADERS, LINKS and COMMANDS" "   " 6
at_xfail=no
(
  $as_echo "237. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >configure.ac <<'_ATEOF'
AC_INIT
AC_PROG_FGREP
rm -f -r header var-header file var-file link var-link command var-command
echo 'OK' >input

# Be sure to also stress the associated INIT-CMDS.
case $what_to_test in
 header)
   AC_CONFIG_HEADERS(header:input);;
 var-header)
   AC_CONFIG_HEADERS(var-header:$header_in, [], [header_in=input]);;

 file)
   AC_CONFIG_FILES(file:input);;
 var-file)
   AC_CONFIG_FILES(var-file:$file_in, [], [file_in=input]);;

 command)
   AC_CONFIG_COMMANDS(command,
		      [cp input command]);;
 var-command)
   AC_CONFIG_COMMANDS(var-command,
		      [cp $command_in var-command], [command_in=input]);;

 link)
   AC_CONFIG_LINKS(link:input);;
 var-link)
   AC_CONFIG_LINKS(var-link:$link_in, [], [link_in=input]);;
esac
AC_OUTPUT
_ATEOF


{ set +x
$as_echo "$at_srcdir/torture.at:221: autoconf --force "
at_fn_check_prepare_trace "torture.at:221"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:221"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "torture.at:221" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/torture.at:221"
{ set +x
$as_echo "$at_srcdir/torture.at:221: /bin/sh -n configure"
at_fn_check_prepare_trace "torture.at:221"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
