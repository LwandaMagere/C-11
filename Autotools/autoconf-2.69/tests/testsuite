$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1319"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

  { set +x
$as_echo "$at_srcdir/tools.at:1320: autoscan -B \"\$dir\""
at_fn_check_prepare_dynamic "autoscan -B \"$dir\"" "tools.at:1320"
( $at_check_trace; autoscan -B "$dir"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1320"
$at_failed && at_fn_log_failure
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/tools.at:1321: autoscan --force -I \"\$dir\""
at_fn_check_prepare_dynamic "autoscan --force -I \"$dir\"" "tools.at:1321"
( $at_check_trace; autoscan --force -I "$dir"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1321"
$at_failed && at_fn_log_failure
$at_traceon; }

  # autoreconf requires a sane input file name.  Also, disable aclocal.
  mv -f "$file.in" configure.in
  : >aclocal.m4
  { set +x
$as_echo "$at_srcdir/tools.at:1325: autoreconf -B \"\$dir\""
at_fn_check_prepare_dynamic "autoreconf -B \"$dir\"" "tools.at:1325"
( $at_check_trace; autoreconf -B "$dir"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1325"
$at_failed && at_fn_log_failure
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/tools.at:1326: autoreconf --force -I \"\$dir\""
at_fn_check_prepare_dynamic "autoreconf --force -I \"$dir\"" "tools.at:1326"
( $at_check_trace; autoreconf --force -I "$dir"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1326"
$at_failed && at_fn_log_failure
$at_traceon; }


  cat >"$file.c" <<'END'
#if FOO
#endif
END
  { set +x
$as_echo "$at_srcdir/tools.at:1332: ifnames \"\$file.c\""
at_fn_check_prepare_dynamic "ifnames \"$file.c\"" "tools.at:1332"
( $at_check_trace; ifnames "$file.c"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:1332"
$at_failed && at_fn_log_failure
$at_traceon; }


  test ! -f b
done

  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
