# Ensure that m4sugar dies when dereferencing undefined macros.

cat >script.4s <<'_ATEOF'
m4_define([good], [yep])
m4_dumpdef([good], [oops])
_ATEOF



{ set +x
$as_echo "$at_srcdir/m4sugar.at:197: autom4te --language=m4sugar script.4s -o script -o-"
