$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


for act_file in state-env.r1 state-env.r2
do
  $SED '/^ac_cv_/ b
	/^[abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_][abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_0123456789]*=./ !d
	/^[^=]*='\'''\''$/ d
	/^a[cs]_/ d
	/^OLDPWD=/ d
	/^PPID=/ d
	/^RANDOM=/ d
	/^SECONDS=/ d
	/'\'\\\$\''=/ d
	/^argv=/ d
	/^ARGC=/ d
	' $act_file >at_config_vars-$act_file
done
{ set +x
$as_echo "$at_srcdir/c.at:75: \$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\""
