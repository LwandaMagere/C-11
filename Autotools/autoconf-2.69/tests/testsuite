cat >configure.ac <<'_ATEOF'
AC_INIT
AC_CONFIG_FILES([at/Makefile paren/Makefile brace/Makefile space/Makefile])
AC_OUTPUT
_ATEOF


cat >at/Makefile.in <<'_ATEOF'
# This is what you should use in order to be portable to old makes.
srcdir = @srcdir@
VPATH = @srcdir@/s1:@srcdir@:@srcdir@/s2
all: f f1 f2
	@echo ok
_ATEOF


cat >paren/Makefile.in <<'_ATEOF'
# This works with some makes but not with old ones.
srcdir = @srcdir@
VPATH = $(srcdir)/s1:$(srcdir):$(srcdir)/s2
all: f f1 f2
	@echo ok
_ATEOF


cat >brace/Makefile.in <<'_ATEOF'
# This works with some makes but not with old ones.
srcdir = @srcdir@
VPATH = ${srcdir}/s1:${srcdir}:${srcdir}/s2
all: f f1 f2
	@echo ok
_ATEOF


cat >space/Makefile.in <<'_ATEOF'
# This fails with FreeBSD make, for example.
srcdir = @srcdir@
VPATH = @srcdir@/s1 @srcdir@ @srcdir@/s2
all: f f1 f2
	@echo ok
_ATEOF


