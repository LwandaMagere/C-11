$at_traceon; }


if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "base.at:404" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/base.at:404"
{ set +x
$as_echo "$at_srcdir/base.at:404: /bin/sh -n configure"
