cat >expout <<'_ATEOF'
/* config.h.  Generated from config.hin by configure.  */
#define foo toto
#  define bar tata
#  define baz titi
  #  define fubar tutu
#define a A
 #define aa AA
 #  define aaa AAA
#define a A
 #  define aa AA
#define aaa AAA
#define aaa AAA
#define aaab
#define aaac(a, aa) aa a
#define multiline line1\
line2\
line3 \
line4
#  define multiline_args(ARG1, ARG2) ARG2 \
ARG1
#define paste(a,b) a##b
/* an ugly one: */
#define str(define) \
#define
#define stringify(arg) str(arg)
#define aaa AAA
/* #undef not_substed */
_ATEOF

{ set +x
$as_echo "$at_srcdir/torture.at:610: cat config.h"
