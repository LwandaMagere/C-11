#AT_START_245
at_fn_group_banner 245 'torture.at:881' \
  "Substitute and define special characters" "       " 6
at_xfail=no
      byte=\\200s; test `{ printf $byte; echo; } | sed -n '/^./p' | wc -l` = 0 && at_xfail=yes
