$at_failed && at_fn_log_failure
$at_traceon; }

fi

 { set +x
$as_echo "$at_srcdir/torture.at:386: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "torture.at:386"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:386"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
