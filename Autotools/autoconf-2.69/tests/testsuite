at_fn_check_status 0 $at_status "$at_srcdir/aclang.at:13"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "aclang.at:13" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/aclang.at:13"
{ set +x
$as_echo "$at_srcdir/aclang.at:13: /bin/sh -n configure"
at_fn_check_prepare_trace "aclang.at:13"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/aclang.at:13"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

{ set +x
$as_echo "$at_srcdir/aclang.at:13: autoheader "
at_fn_check_prepare_trace "aclang.at:13"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/aclang.at:13"
$at_failed && at_fn_log_failure
$at_traceon; }



for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/aclang.at:13: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "aclang.at:13"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/aclang.at:13"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/aclang.at:13: at_check_env"
at_fn_check_prepare_trace "aclang.at:13"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/aclang.at:13"
$at_failed && at_fn_log_failure  \
