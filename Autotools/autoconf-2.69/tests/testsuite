$as_echo "$at_srcdir/acc.at:28: \$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\""
at_fn_check_prepare_dynamic "$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\"" "acc.at:28"
( $at_check_trace; $at_diff "at_config_vars-state-env.r1" "at_config_vars-state-env.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/acc.at:28"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }





