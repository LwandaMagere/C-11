$as_echo "$at_srcdir/autotest.at:852: sed -n 's/[^.]*\\. \\(.*\\) ([^)]*): ok.*/\\1/p' micro-suite.log"
at_fn_check_prepare_trace "autotest.at:852"
( $at_check_trace; sed -n 's/[^.]*\. \(.*\) ([^)]*): ok.*/\1/p' micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
