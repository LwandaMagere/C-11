done

{ set +x
$as_echo "$at_srcdir/semantics.at:343: \$at_diff \"config-h.r1\" \"config-h.r2\""
at_fn_check_prepare_dynamic "$at_diff \"config-h.r1\" \"config-h.r2\"" "semantics.at:343"
( $at_check_trace; $at_diff "config-h.r1" "config-h.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
