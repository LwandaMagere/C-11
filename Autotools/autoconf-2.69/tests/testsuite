$as_echo "$at_srcdir/autotest.at:846: \$CONFIG_SHELL ./micro-suite -l |
	   sed -n 's/.*[0-9]: [^	 ][^	 ]*[	 ][	 ]*\\(.*[^	 ]\\)[	 ]*/\\1/p'"
at_fn_check_prepare_notrace 'an embedded newline' "autotest.at:846"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -l |
	   sed -n 's/.*[0-9]: [^	 ][^	 ]*[	 ][	 ]*\(.*[^	 ]\)[	 ]*/\1/p'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
