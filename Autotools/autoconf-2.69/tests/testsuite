$at_traceon; }

   { set +x
$as_echo "$at_srcdir/autotest.at:1495: grep \"[12] .* inhibited subsequent\" stderr"
at_fn_check_prepare_trace "autotest.at:1495"
( $at_check_trace; grep "[12] .* inhibited subsequent" stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1495"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
#AT_STOP_208
#AT_START_209
at_fn_group_banner 209 'autotest.at:1511' \
  "parallel autotest and signal handling" "          " 4
at_xfail=no
(
  $as_echo "209. $at_setup_line: testing $at_desc ..."
  $at_traceon


# Per BUGS, we have not yet figured out how to run parallel tests cleanly
# under dash and some ksh variants.  For now, only run this test under
# limited conditions; help is appreciated in widening this test base.
$as_echo "autotest.at:1513" >"$at_check_line_file"
(${CONFIG_SHELL-$SHELL} -c 'test -z "${BASH_VERSION+set}${ZSH_VERSION+set}${TEST_PARALLEL_AUTOTEST+set}"') \
  && at_fn_check_skip 77 "$at_srcdir/autotest.at:1513"
# The parallel scheduler requires mkfifo and job control to work.
{ set +x
$as_echo "$at_srcdir/autotest.at:1513: mkfifo fifo || exit 77"
at_fn_check_prepare_trace "autotest.at:1513"
( $at_check_trace; mkfifo fifo || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1513"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1513: \${CONFIG_SHELL-\$SHELL} -c '(set -m && set +m) || exit 77'"
at_fn_check_prepare_notrace 'a ${...} parameter expansion' "autotest.at:1513"
( $at_check_trace; ${CONFIG_SHELL-$SHELL} -c '(set -m && set +m) || exit 77'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1513"
$at_failed && at_fn_log_failure
$at_traceon; }



# Goals:
# (1) interrupt `./testsuite -jN'
# (2) interrupt `make check TESTSUITEFLAGS=-jN'
# (3) no trailing verbose/trace output
# (4) exit status should be 128+signal

cat >atlocal <<'_ATEOF'
suite_pid=$$
export suite_pid
_ATEOF


cat >./package.m4 <<'_ATEOF'
m4_define([AT_PACKAGE_NAME],      [GNU Nonsense])
m4_define([AT_PACKAGE_TARNAME],   [nonsense])
m4_define([AT_PACKAGE_VERSION],   [1.0])
m4_define([AT_PACKAGE_STRING],    [GNU Nonsense 1.0])
m4_define([AT_PACKAGE_BUGREPORT], [bug-autoconf@gnu.org])
_ATEOF

cat >./micro-suite.at <<'_ATEOF'
AT_INIT([suite to test parallel execution])
AT_SETUP([test number 1])
AT_CHECK([sleep 2])
AT_CLEANUP
AT_SETUP([test number 2])
AT_CHECK([sleep 1])
AT_CLEANUP
AT_SETUP([test number 3])
AT_CHECK([sleep 1])
AT_CLEANUP
AT_SETUP([killer test])
AT_CHECK([kill -$signal $suite_pid])
AT_CLEANUP
m4_for([count], [5], [7], [],
   [AT_SETUP([test number count])
    AT_CHECK([sleep 1])
    AT_CLEANUP
])
_ATEOF

{ set +x
$as_echo "$at_srcdir/autotest.at:1526: cat >./atconfig <<EOF
at_testdir=.
abs_builddir='\`pwd\`'
at_srcdir=.
abs_srcdir='\`pwd\`'
at_top_srcdir=.
abs_top_srcdir='\`pwd\`'
at_top_build_prefix=
abs_top_builddir='\`pwd\`'
EOF"
at_fn_check_prepare_notrace 'a `...` command substitution' "autotest.at:1526"
( $at_check_trace; cat >./atconfig <<EOF
at_testdir=.
abs_builddir='`pwd`'
at_srcdir=.
abs_srcdir='`pwd`'
at_top_srcdir=.
abs_top_srcdir='`pwd`'
at_top_build_prefix=
abs_top_builddir='`pwd`'
EOF
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1526"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/autotest.at:1526: autom4te --language=autotest -o micro-suite micro-suite.at"
at_fn_check_prepare_trace "autotest.at:1526"
( $at_check_trace; autom4te --language=autotest -o micro-suite micro-suite.at
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1526"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }






cat >Makefile.in <<'_ATEOF'
@SET_MAKE@
SHELL = @SHELL@
TESTSUITE = ./micro-suite
check:
	$(SHELL) '$(TESTSUITE)' $(TESTSUITEFLAGS)
.PHONY: check
_ATEOF


{ set +x
$as_echo "$at_srcdir/autotest.at:1556: \$CONFIG_SHELL \$abs_top_builddir/config.status --file=Makefile:Makefile.in"
at_fn_check_prepare_dynamic "$CONFIG_SHELL $abs_top_builddir/config.status --file=Makefile:Makefile.in" "autotest.at:1556"
( $at_check_trace; $CONFIG_SHELL $abs_top_builddir/config.status --file=Makefile:Makefile.in
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1556"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


# Test INT and TERM.
for signal in 2 15; do
  export signal
  as_fn_arith 128 + $signal && expected_status=$as_val

  # Sequential case.
  { set +x
$as_echo "$at_srcdir/autotest.at:1565: \$CONFIG_SHELL ./micro-suite"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite" "autotest.at:1565"
( $at_check_trace; $CONFIG_SHELL ./micro-suite
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status $expected_status $at_status "$at_srcdir/autotest.at:1565"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  # Both stderr and the log should contain the notification about the signal.
  { set +x
$as_echo "$at_srcdir/autotest.at:1568: grep 'bailing out' stderr"
at_fn_check_prepare_trace "autotest.at:1568"
( $at_check_trace; grep 'bailing out' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1568"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1569: grep 'bailing out' micro-suite.log"
at_fn_check_prepare_trace "autotest.at:1569"
( $at_check_trace; grep 'bailing out' micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1569"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  # There should be no junk job status output.
  { set +x
$as_echo "$at_srcdir/autotest.at:1571: grep '[iI]nterrupt[	 ]' stderr"
at_fn_check_prepare_trace "autotest.at:1571"
( $at_check_trace; grep '[iI]nterrupt[	 ]' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1571"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


  # Parallel case.
  { set +x
$as_echo "$at_srcdir/autotest.at:1574: \$CONFIG_SHELL ./micro-suite --jobs=3"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite --jobs=3" "autotest.at:1574"
( $at_check_trace; $CONFIG_SHELL ./micro-suite --jobs=3
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status $expected_status $at_status "$at_srcdir/autotest.at:1574"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1576: grep 'bailing out' stderr"
at_fn_check_prepare_trace "autotest.at:1576"
( $at_check_trace; grep 'bailing out' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1576"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1577: grep 'bailing out' micro-suite.log"
at_fn_check_prepare_trace "autotest.at:1577"
( $at_check_trace; grep 'bailing out' micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1577"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  # We'd like to check this here, too, but some shells do not allow to
  # turn off job control.
  # AT_CHECK([[grep '[iI]nterrupt[	 ]' stderr]], [1])

  # Ditto with `make' in the loop.
  : "${MAKE=make}"
  unset MAKEFLAGS
  # Need to eliminate outer TESTSUITEFLAGS here.
  # Need to normalize exit status here: some make implementations
  # exit 1 (BSD make), some exit 2 (GNU make).
  { set +x
$as_echo "$at_srcdir/autotest.at:1588: \$MAKE check TESTSUITEFLAGS=; 	   case \$? in 1|2) exit 1;; *) exit \$?;; esac"
at_fn_check_prepare_notrace 'a shell pipeline' "autotest.at:1588"
( $at_check_trace; $MAKE check TESTSUITEFLAGS=; 	   case $? in 1|2) exit 1;; *) exit $?;; esac
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1588"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1591: grep 'bailing out' stderr"
at_fn_check_prepare_trace "autotest.at:1591"
( $at_check_trace; grep 'bailing out' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1591"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1592: grep 'bailing out' micro-suite.log"
at_fn_check_prepare_trace "autotest.at:1592"
( $at_check_trace; grep 'bailing out' micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1592"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  # Ditto, parallel case.
  { set +x
$as_echo "$at_srcdir/autotest.at:1594: \$MAKE check TESTSUITEFLAGS=--jobs=3; 	   case \$? in 1|2) exit 1;; *) exit \$?;; esac"
at_fn_check_prepare_notrace 'a shell pipeline' "autotest.at:1594"
( $at_check_trace; $MAKE check TESTSUITEFLAGS=--jobs=3; 	   case $? in 1|2) exit 1;; *) exit $?;; esac
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1594"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1597: grep 'bailing out' stderr"
at_fn_check_prepare_trace "autotest.at:1597"
( $at_check_trace; grep 'bailing out' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1597"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/autotest.at:1598: grep 'bailing out' micro-suite.log"
at_fn_check_prepare_trace "autotest.at:1598"
( $at_check_trace; grep 'bailing out' micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1598"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

done


# Test PIPE.
# The most important part here is that things should not hang, nor
# get out of hand.  OTOH, if the shell sets the default handler to
# ignore PIPE (pdksh, dash), there is little we can do about having the
# test run; it's only the output that won't be there.  So all we check
# for is that, if test 7 didn't run serially, then it shouldn't be
# run in the parallel case either; the intermediate tests serve as
# parallel barrier.
# Note that stderr may contain "Broken pipe" errors.
{ set +x
$as_echo "$at_srcdir/autotest.at:1611: (\$CONFIG_SHELL ./micro-suite -d -3 5-; echo \$? >status) | sed 5q"
at_fn_check_prepare_notrace 'a shell pipeline' "autotest.at:1611"
( $at_check_trace; ($CONFIG_SHELL ./micro-suite -d -3 5-; echo $? >status) | sed 5q
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1611"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1613: grep '5.*ok' stdout"
at_fn_check_prepare_trace "autotest.at:1613"
( $at_check_trace; grep '5.*ok' stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1613"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

# Apparently some shells don't get around to creating 'status' any more.
# And ksh93 on FreeBSD uses 256 + 13 instead of 128 + 13
{ set +x
$as_echo "$at_srcdir/autotest.at:1616: test ! -s status || grep 141 status || grep 269 status"
at_fn_check_prepare_trace "autotest.at:1616"
( $at_check_trace; test ! -s status || grep 141 status || grep 269 status
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1616"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1618: if test -f micro-suite.dir/7/micro-suite.log; then 	   echo \"shell ignores SIGPIPE\" > sigpipe-stamp 	 else :; fi"
at_fn_check_prepare_trace "autotest.at:1618"
( $at_check_trace; if test -f micro-suite.dir/7/micro-suite.log; then 	   echo "shell ignores SIGPIPE" > sigpipe-stamp 	 else :; fi
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1618"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/autotest.at:1622: \$CONFIG_SHELL ./micro-suite -d -3 5- --jobs=2 | sed 5q"
at_fn_check_prepare_notrace 'a shell pipeline' "autotest.at:1622"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -d -3 5- --jobs=2 | sed 5q
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1622"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1623: grep '5.*ok' stdout"
at_fn_check_prepare_trace "autotest.at:1623"
( $at_check_trace; grep '5.*ok' stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1623"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1624: test -s sigpipe-stamp || test ! -f micro-suite.dir/7/micro-suite.log"
at_fn_check_prepare_trace "autotest.at:1624"
( $at_check_trace; test -s sigpipe-stamp || test ! -f micro-suite.dir/7/micro-suite.log
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1624"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
#AT_STOP_209
#AT_START_210
at_fn_group_banner 210 'autotest.at:1630' \
  "parallel args but non-working mkfifo" "           " 4
at_xfail=no
(
  $as_echo "210. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >./package.m4 <<'_ATEOF'
m4_define([AT_PACKAGE_NAME],      [GNU Nonsense])
m4_define([AT_PACKAGE_TARNAME],   [nonsense])
m4_define([AT_PACKAGE_VERSION],   [1.0])
m4_define([AT_PACKAGE_STRING],    [GNU Nonsense 1.0])
m4_define([AT_PACKAGE_BUGREPORT], [bug-autoconf@gnu.org])
_ATEOF

cat >./micro-suite.at <<'_ATEOF'

AT_INIT([artificial test suite])
AT_SETUP([my only test])
AT_CHECK([:])
AT_CLEANUP
AT_SETUP([second test])
AT_CHECK([:])

AT_CLEANUP
_ATEOF

{ set +x
$as_echo "$at_srcdir/autotest.at:1630: cat >./atconfig <<EOF
at_testdir=.
abs_builddir='\`pwd\`'
at_srcdir=.
abs_srcdir='\`pwd\`'
at_top_srcdir=.
abs_top_srcdir='\`pwd\`'
at_top_build_prefix=
abs_top_builddir='\`pwd\`'
EOF"
at_fn_check_prepare_notrace 'a `...` command substitution' "autotest.at:1630"
( $at_check_trace; cat >./atconfig <<EOF
at_testdir=.
abs_builddir='`pwd`'
at_srcdir=.
abs_srcdir='`pwd`'
at_top_srcdir=.
abs_top_srcdir='`pwd`'
at_top_build_prefix=
abs_top_builddir='`pwd`'
EOF
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1630"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/autotest.at:1630: autom4te --language=autotest -o micro-suite micro-suite.at"
at_fn_check_prepare_trace "autotest.at:1630"
( $at_check_trace; autom4te --language=autotest -o micro-suite micro-suite.at
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1630"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }





# Per BUGS, we have not yet figured out how to run parallel tests cleanly
# under dash and some ksh variants.  For now, only run this test under
# limited conditions; help is appreciated in widening this test base.
$as_echo "autotest.at:1630" >"$at_check_line_file"
(${CONFIG_SHELL-$SHELL} -c 'test -z "${BASH_VERSION+set}${ZSH_VERSION+set}${TEST_PARALLEL_AUTOTEST+set}"') \
  && at_fn_check_skip 77 "$at_srcdir/autotest.at:1630"
# The parallel scheduler requires mkfifo and job control to work.
{ set +x
$as_echo "$at_srcdir/autotest.at:1630: mkfifo fifo || exit 77"
at_fn_check_prepare_trace "autotest.at:1630"
( $at_check_trace; mkfifo fifo || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1630"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1630: \${CONFIG_SHELL-\$SHELL} -c '(set -m && set +m) || exit 77'"
at_fn_check_prepare_notrace 'a ${...} parameter expansion' "autotest.at:1630"
( $at_check_trace; ${CONFIG_SHELL-$SHELL} -c '(set -m && set +m) || exit 77'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1630"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


mkdir bin
cat >bin/mkfifo <<\EOF
#! /bin/sh
exit 1
EOF
chmod +x bin/mkfifo
PATH=`pwd`/bin:$PATH
export PATH

{ set +x
$as_echo "$at_srcdir/autotest.at:1630: \$CONFIG_SHELL ./micro-suite --jobs"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite --jobs" "autotest.at:1630"
( $at_check_trace; $CONFIG_SHELL ./micro-suite --jobs
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
