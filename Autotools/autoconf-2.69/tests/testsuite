at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1614"
$at_failed && at_fn_log_failure
$at_traceon; }


# Running the outer configure recursively should provide the innermost
# help strings.
chmod a-w inner/innermost inner
{ set +x
$as_echo "$at_srcdir/torture.at:1619: { ./configure \$configure_options --help=recursive; chmod u+w .; } | grep \" INNER \""
at_fn_check_prepare_notrace 'a shell pipeline' "torture.at:1619"
( $at_check_trace; { ./configure $configure_options --help=recursive; chmod u+w .; } | grep " INNER "
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1619"
if $at_failed; then :
  { set +x
$as_echo "$at_srcdir/torture.at:1619: grep 'rerun with a POSIX shell' stderr"
at_fn_check_prepare_trace "torture.at:1619"
( $at_check_trace; grep 'rerun with a POSIX shell' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1619"
$at_failed && at_fn_log_failure
$at_traceon; }

fi
$at_failed && at_fn_log_failure
$at_traceon; }

