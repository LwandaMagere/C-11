/a\\1--/b\\1--/c\\1
::4
:4
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/m4sugar.at:1450"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
