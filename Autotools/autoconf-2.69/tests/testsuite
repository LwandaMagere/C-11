  $as_echo "217. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >configure.ac <<'_ATEOF'
AC_REQUIRE([AC_PROG_CC])
_ATEOF


{ set +x
$as_echo "$at_srcdir/base.at:78: autoconf --force "
at_fn_check_prepare_trace "base.at:78"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/base.at:78"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/base.at:78: sed 's/^[^:]*m4[-.ex0-9]*: *\\([^:]*:\\) *\\([0-9][0-9]*: \\)/m4:\\1\\2/
	s/^\\([^:]*:\\) *\\([0-9][0-9]*:\\)[^:]*m4[-.ex0-9]*: /m4:\\1\\2 /
	s/: C\\(annot open \\)\\([^\`:]*\\):/: c\\1\`\\2'\\'':/
	s/: include:\\( cannot open\\)/:\\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)\$//
    ' stderr >&2"
at_fn_check_prepare_notrace 'a `...` command substitution' "base.at:78"
( $at_check_trace; sed 's/^[^:]*m4[-.ex0-9]*: *\([^:]*:\) *\([0-9][0-9]*: \)/m4:\1\2/
	s/^\([^:]*:\) *\([0-9][0-9]*:\)[^:]*m4[-.ex0-9]*: /m4:\1\2 /
	s/: C\(annot open \)\([^`:]*\):/: c\1`\2'\'':/
	s/: include:\( cannot open\)/:\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)$//
    ' stderr >&2
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo >>"$at_stderr"; $as_echo "configure.ac:1: error: AC_REQUIRE(AC_PROG_CC): cannot be used outside of an AC_DEFUN'd macro
configure.ac:1: the top level
autom4te: m4 failed with exit status: 1
" | \
  $at_diff - "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/base.at:78"
$at_failed && at_fn_log_failure
$at_traceon; }


if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "base.at:78" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/base.at:78"
{ set +x
$as_echo "$at_srcdir/base.at:78: /bin/sh -n configure"
at_fn_check_prepare_trace "base.at:78"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/base.at:78"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
