		  AC_MSG_ERROR([cannot find `printf']))
AC_TRY_LINK_FUNC(Be_doomed_if_your_libc_has_a_function_named_like_this,
		 AC_MSG_ERROR([found a nonexistent function]))
AC_OUTPUT
AC_STATE_SAVE(after)
_ATEOF

