at_top_build_prefix=
abs_top_builddir='\`pwd\`'
EOF"
at_fn_check_prepare_notrace 'a `...` command substitution' "autotest.at:870"
( $at_check_trace; cat >./atconfig <<EOF
at_testdir=.
abs_builddir='`pwd`'
at_srcdir=.
abs_srcdir='`pwd`'
at_top_srcdir=.
abs_top_srcdir='`pwd`'
at_top_build_prefix=
abs_top_builddir='`pwd`'
EOF
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:870"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/autotest.at:870: autom4te --language=autotest -o micro-suite micro-suite.at"
at_fn_check_prepare_trace "autotest.at:870"
( $at_check_trace; autom4te --language=autotest -o micro-suite micro-suite.at
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:870"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }






{ set +x
$as_echo "$at_srcdir/autotest.at:870: \$CONFIG_SHELL ./micro-suite "
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite " "autotest.at:870"
( $at_check_trace; $CONFIG_SHELL ./micro-suite
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
