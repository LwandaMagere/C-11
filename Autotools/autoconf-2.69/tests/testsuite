at_xfail=no
(
  $as_echo "255. $at_setup_line: testing $at_desc ..."
  $at_traceon



# We use aclocal (via autoreconf).
{ set +x
$as_echo "$at_srcdir/torture.at:1433: aclocal --version || exit 77"
at_fn_check_prepare_trace "torture.at:1433"
( $at_check_trace; aclocal --version || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1433"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:1434: grep '[1-9]\\.[0-9]' stdout || exit 77"
at_fn_check_prepare_trace "torture.at:1434"
( $at_check_trace; grep '[1-9]\.[0-9]' stdout || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1434"
$at_failed && at_fn_log_failure
$at_traceon; }


# It should understand configure.ac.
{ set +x
$as_echo "$at_srcdir/torture.at:1437: grep '[^0-9]1\\.[01234][^0-9]' stdout && exit 77"
at_fn_check_prepare_trace "torture.at:1437"
( $at_check_trace; grep '[^0-9]1\.[01234][^0-9]' stdout && exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:1437"
$at_failed && at_fn_log_failure
$at_traceon; }


# Set CONFIG_SITE to a nonexistent file, so that there are
# no worries about nonstandard values for 'prefix'.
CONFIG_SITE=no-such-file
export CONFIG_SITE

# The contents of `inner/', and `inner/innermost/'.
as_dir=inner/innermost; as_fn_mkdir_p

# We have to use configure.in, not configure.ac, if we still want to
# be compatible with Automake 1.4: aclocal (run by autoreconf) would
# die because it can't find configure.in.
cat >inner/configure.in <<'_ATEOF'
AC_INIT(GNU Inner, 1.0)
AC_CONFIG_SRCDIR([innermost/config.in])
AC_ARG_VAR([INNER], [an inner variable])
AC_SUBST([INNER])
if test "x$INNER" = x; then
  INNER=inner
fi
AC_CONFIG_FILES([innermost/config])
AC_OUTPUT
_ATEOF


cat >inner/innermost/config.in <<'_ATEOF'
INNER=@INNER@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
_ATEOF


# The contents of `.'
: >install-sh

# nonexistent is allowed not to exist.
cat >configure.in <<'_ATEOF'
AC_INIT(GNU Outer, 1.0)
AC_ARG_VAR([OUTER], [an outer variable])
if false; then
  AC_CONFIG_SUBDIRS([nonexistent])
fi
AC_CONFIG_SUBDIRS([inner])
AC_OUTPUT
_ATEOF


# If there are improperly quoted AC_DEFUN installed in share/aclocal,
# they trigger warnings from aclocal 1.8, so ignore stderr.
{ set +x
$as_echo "$at_srcdir/torture.at:1485: autoreconf"
at_fn_check_prepare_trace "torture.at:1485"
( $at_check_trace; autoreconf
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1485"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:1486: test -f inner/configure"
at_fn_check_prepare_trace "torture.at:1486"
( $at_check_trace; test -f inner/configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1486"
$at_failed && at_fn_log_failure
$at_traceon; }


# Running the outer configure recursively should provide the innermost
# help strings.
chmod a-w inner/innermost inner .
{ set +x
$as_echo "$at_srcdir/torture.at:1491: { ./configure \$configure_options --help=recursive; chmod u+w .; } | grep INNER"
at_fn_check_prepare_notrace 'a shell pipeline' "torture.at:1491"
( $at_check_trace; { ./configure $configure_options --help=recursive; chmod u+w .; } | grep INNER
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1491"
if $at_failed; then :
  { set +x
$as_echo "$at_srcdir/torture.at:1491: grep 'rerun with a POSIX shell' stderr"
at_fn_check_prepare_trace "torture.at:1491"
( $at_check_trace; grep 'rerun with a POSIX shell' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1491"
$at_failed && at_fn_log_failure
$at_traceon; }

fi
$at_failed && at_fn_log_failure
$at_traceon; }

chmod u+w . inner inner/innermost

# Running the outer configure should trigger the inner.
 { set +x
$as_echo "$at_srcdir/torture.at:1497: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "torture.at:1497"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1497"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:1498: cat inner/innermost/config"
at_fn_check_prepare_trace "torture.at:1498"
( $at_check_trace; cat inner/innermost/config
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "INNER=inner
srcdir=.
top_srcdir=..
prefix=/usr/local
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:1498"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


