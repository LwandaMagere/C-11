# Check that pushdef stacks can be dumped.

cat >script.4s <<'_ATEOF'
m4_init
m4_divert_push([])[]dnl
m4_divert_push([KILL])
m4_pushdef([a], [1])
m4_pushdef([a], [2])
m4_dumpdef([a])
m4_dumpdefs([oops], [a])
m4_divert_pop([KILL])dnl
[]dnl
m4_divert_pop([])
_ATEOF



