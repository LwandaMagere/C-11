at_fn_check_status 0 $at_status "$at_srcdir/torture.at:296"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:298: grep from-stdin file"
