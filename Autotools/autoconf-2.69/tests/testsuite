) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:385"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

   { set +x
$as_echo "$at_srcdir/autotest.at:385: grep \"inhibited subsequent\" stderr"
at_fn_check_prepare_trace "autotest.at:385"
( $at_check_trace; grep "inhibited subsequent" stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
