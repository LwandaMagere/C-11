  $at_traceon



cat >configure.ac <<'_ATEOF'
AC_INIT
AC_CONFIG_HEADERS(config.h:config.hin)
AC_STATE_SAVE(before)
AC_MEMORY_H
AC_OUTPUT
AC_STATE_SAVE(after)
_ATEOF

cp "$abs_top_srcdir/build-aux/install-sh" \
   "$abs_top_srcdir/build-aux/config.guess" \
   "$abs_top_srcdir/build-aux/config.sub" .
cp "$abs_top_srcdir/tests/statesave.m4" aclocal.m4


{ set +x
$as_echo "$at_srcdir/acheaders.at:24: autoconf --force "
at_fn_check_prepare_trace "acheaders.at:24"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/acheaders.at:24"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "acheaders.at:24" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/acheaders.at:24"
{ set +x
$as_echo "$at_srcdir/acheaders.at:24: /bin/sh -n configure"
at_fn_check_prepare_trace "acheaders.at:24"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/acheaders.at:24"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

{ set +x
$as_echo "$at_srcdir/acheaders.at:24: autoheader "
at_fn_check_prepare_trace "acheaders.at:24"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/acheaders.at:24"
$at_failed && at_fn_log_failure
$at_traceon; }


 { set +x
$as_echo "$at_srcdir/acheaders.at:24: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "acheaders.at:24"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/acheaders.at:24"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/acheaders.at:24: at_check_env"
at_fn_check_prepare_trace "acheaders.at:24"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
