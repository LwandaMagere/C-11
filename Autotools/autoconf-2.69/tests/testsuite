# config.status might be stupidly expecting data on stdin, if it's
# really broken...
# Skip check if user can rename files into a read-only directory (when
# run by root or on w32).
touch t
chmod a-w .
mv t t1 >/dev/null 2>&1 \
  || { set +x
$as_echo "$at_srcdir/torture.at:294: ./config.status var-file </dev/null || exit 1"
at_fn_check_prepare_trace "torture.at:294"
( $at_check_trace; ./config.status var-file </dev/null || exit 1
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:294"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

chmod u+w .
rm -rf t t1

# Create a file with bits from stdin
{ set +x
$as_echo "$at_srcdir/torture.at:296: echo from-stdin | ./config.status --file=file:-"
at_fn_check_prepare_notrace 'a shell pipeline' "torture.at:296"
( $at_check_trace; echo from-stdin | ./config.status --file=file:-
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
