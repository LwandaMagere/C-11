437;acheaders.at:19;AC_HEADER_STDBOOL;;
438;acheaders.at:20;AC_HEADER_TIOCGWINSZ;;
439;acheaders.at:23;AC_DIR_HEADER;autoupdate;
440;acheaders.at:24;AC_MEMORY_H;autoupdate;
441;acheaders.at:25;AC_UNISTD_H;autoupdate;
442;acheaders.at:26;AC_USG;autoupdate;
443;actypes.at:13;AC_STRUCT_DIRENT_D_INO;;
444;actypes.at:14;AC_STRUCT_DIRENT_D_TYPE;;
445;actypes.at:15;AC_STRUCT_ST_BLOCKS;;
446;actypes.at:16;AC_STRUCT_TIMEZONE;;
447;actypes.at:17;AC_TYPE_INT16_T;;
448;actypes.at:18;AC_TYPE_INT32_T;;
449;actypes.at:19;AC_TYPE_INT64_T;;
450;actypes.at:20;AC_TYPE_INT8_T;;
