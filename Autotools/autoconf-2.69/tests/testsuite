# --cache-file is adjusted for subdirectories, so the cache is shared.
{ set +x
$as_echo "$at_srcdir/torture.at:1542: cd builddir && ../configure \$configure_options --config-cache"
at_fn_check_prepare_dynamic "cd builddir && ../configure $configure_options --config-cache" "torture.at:1542"
