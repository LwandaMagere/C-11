cat >configure.ac <<'_ATEOF'
AC_INIT([pkg], [1.0])
AC_PROG_CC
AC_COMPILE_IFELSE([AC_LANG_SOURCE([[#ifndef PACKAGE_NAME
choke me
#endif
int main ()
{
  return 0;
}
]], [], [AC_MSG_FAILURE([confdefs not included])])])
