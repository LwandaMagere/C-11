at_top_build_prefix=
abs_top_builddir='`pwd`'
EOF
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:833"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/autotest.at:833: autom4te --language=autotest -o micro-suite micro-suite.at"
at_fn_check_prepare_trace "autotest.at:833"
( $at_check_trace; autom4te --language=autotest -o micro-suite micro-suite.at
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:833"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }






{ set +x
$as_echo "$at_srcdir/autotest.at:833: \$CONFIG_SHELL ./micro-suite "
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite " "autotest.at:833"
( $at_check_trace; $CONFIG_SHELL ./micro-suite
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:833"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:833: \$CONFIG_SHELL ./micro-suite -v -x "
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite -v -x " "autotest.at:833"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -v -x
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
