cat >configure.ac <<'_ATEOF'
AC_INIT([shtool test], [1.0])
AC_CONFIG_AUX_DIR([build-aux])
AC_PATH_PROG([SHTOOL], [shtool], [false])
AC_PROG_INSTALL
AC_SUBST([ac_install_sh])
AC_CONFIG_FILES([Makefile])
AC_OUTPUT
_ATEOF


cat >Makefile.in <<'_ATEOF'
SHTOOL = @SHTOOL@
# fake these, so that we use shtool wherever possible.
INSTALL_PROGRAM = $(ac_install_sh)
INSTALL_DATA = $(ac_install_sh) -m 644
prefix = @prefix@
ac_install_sh = @ac_install_sh@

install:
	$(INSTALL_PROGRAM) file1 $(prefix)/file1
	$(INSTALL_DATA) file2 $(prefix)/file2

copy-shtool:
	test '$(SHTOOL)' != false
	cp '$(SHTOOL)' build-aux
_ATEOF


: >file1
: >file2
chmod +x file1
: "${MAKE=make}"
mkdir build-aux inst
instdir=`pwd`/inst
{ set +x
$as_echo "$at_srcdir/foreign.at:127: autoconf --force "
at_fn_check_prepare_trace "foreign.at:127"
