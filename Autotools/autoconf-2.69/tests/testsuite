$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:221"
$at_failed && at_fn_log_failure
$at_traceon; }

fi



# AT_CHECK_CONFIG_CREATION(THING = (header | link | file | command))
# ------------------------------------------------------------------
# Check that THING and var-THING (which uses variables in AC_CONFIG_THING)
# are properly created, with the right content.
# Use `grep OK' instead of a simple `cat' to avoid banners such as in
# AC_CONFIG_HEADERS.
# AT_CHECK_CONFIG_CREATION


# AT_CHECK_CONFIG_CREATION_NOWRITE(THING = (header | link | file | command))
# --------------------------------------------------------------------------
# Check that THING and var-THING (which uses variables in AC_CONFIG_THING)
# are properly created, with the right content.
# Use `grep OK' instead of a simple `cat' to avoid banners such as in
# AC_CONFIG_HEADERS.
# AT_CHECK_CONFIG_CREATION_NOWRITE


# Create a file
 { set +x
$as_echo "$at_srcdir/torture.at:280: ./configure \$configure_options what_to_test=file"
at_fn_check_prepare_dynamic "./configure $configure_options what_to_test=file" "torture.at:280"
( $at_check_trace; ./configure $configure_options what_to_test=file
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:280"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:280: ls header var-header file var-file command var-command link var-link 2>/dev/null"
at_fn_check_prepare_trace "torture.at:280"
( $at_check_trace; ls header var-header file var-file command var-command link var-link 2>/dev/null
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "file
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_skip $at_status "$at_srcdir/torture.at:280"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:280: grep OK file"
at_fn_check_prepare_trace "torture.at:280"
( $at_check_trace; grep OK file
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "OK
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:280"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


 { set +x
$as_echo "$at_srcdir/torture.at:280: ./configure \$configure_options what_to_test=var-file --no-create"
at_fn_check_prepare_dynamic "./configure $configure_options what_to_test=var-file --no-create" "torture.at:280"
( $at_check_trace; ./configure $configure_options what_to_test=var-file --no-create
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
