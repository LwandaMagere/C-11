# Ensure the examples from the manual work as intended.
# Taken from autoconf.texi:Caching Results
cat >configure.ac <<'_ATEOF'
AC_INIT
AC_DEFUN([AC_SHELL_TRUE],
[AC_CACHE_CHECK([whether true(1) works], [my_cv_shell_true_works],
                [my_cv_shell_true_works=no
                 (true) 2>/dev/null && my_cv_shell_true_works=yes
                 if test "x$my_cv_shell_true_works" = xyes; then
                   AC_DEFINE([TRUE_WORKS], [1],
                             [Define if `true(1)' works properly.])
                 fi])
])
AC_SHELL_TRUE
_ATEOF

{ set +x
$as_echo "$at_srcdir/base.at:422: autoconf --force -Werror"
at_fn_check_prepare_trace "base.at:422"
( $at_check_trace; autoconf --force -Werror
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/base.at:422"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/base.at:422: sed 's/^[^:]*m4[-.ex0-9]*: *\\([^:]*:\\) *\\([0-9][0-9]*: \\)/m4:\\1\\2/
	s/^\\([^:]*:\\) *\\([0-9][0-9]*:\\)[^:]*m4[-.ex0-9]*: /m4:\\1\\2 /
	s/: C\\(annot open \\)\\([^\`:]*\\):/: c\\1\`\\2'\\'':/
	s/: include:\\( cannot open\\)/:\\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)\$//
    ' stderr >&2"
at_fn_check_prepare_notrace 'a `...` command substitution' "base.at:422"
( $at_check_trace; sed 's/^[^:]*m4[-.ex0-9]*: *\([^:]*:\) *\([0-9][0-9]*: \)/m4:\1\2/
	s/^\([^:]*:\) *\([0-9][0-9]*:\)[^:]*m4[-.ex0-9]*: /m4:\1\2 /
	s/: C\(annot open \)\([^`:]*\):/: c\1`\2'\'':/
	s/: include:\( cannot open\)/:\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)$//
    ' stderr >&2
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/base.at:422"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


