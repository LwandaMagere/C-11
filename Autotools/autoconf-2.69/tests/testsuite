TERM=ansi
export TERM

red=`printf '\033[0;31m'`
grn=`printf '\033[0;32m'`
lgn=`printf '\033[1;32m'`
blu=`printf '\033[1;34m'`
std=`printf '\033[m'`

# Check that grep can parse nonprinting characters.
# BSD 'grep' works from a pipe, but not a seekable file.
# GNU or BSD 'grep -a' works on files, but is not portable.
{ set +x
$as_echo "$at_srcdir/autotest.at:1652: case \`echo \"\$std\" | grep .\` in #'' restore font-lock
	    \$std) :;;
	    *) exit 77;;
	  esac"
at_fn_check_prepare_notrace 'a `...` command substitution' "autotest.at:1652"
( $at_check_trace; case `echo "$std" | grep .` in #'' restore font-lock
	    $std) :;;
	    *) exit 77;;
	  esac
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1652"
if $at_failed; then :
  echo "grep can't parse nonprinting characters" >&2
fi
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


if echo 'ab*c' | grep -F 'ab*c' >/dev/null 2>&1; then
  FGREP="grep -F"
else
  FGREP=fgrep
fi

# No color.
{ set +x
$as_echo "$at_srcdir/autotest.at:1652: \$CONFIG_SHELL ./micro-suite"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite" "autotest.at:1652"
( $at_check_trace; $CONFIG_SHELL ./micro-suite
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
