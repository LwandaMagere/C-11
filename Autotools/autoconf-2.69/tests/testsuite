#AT_START_307
at_fn_group_banner 307 'fortran.at:31' \
  "GNU Fortran 77" "                                 " 12
at_xfail=no
(
  $as_echo "307. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >configure.ac <<'_ATEOF'
AC_INIT
AC_CONFIG_HEADERS(config.h:config.hin)
AC_STATE_SAVE(before)
AC_LANG(Fortran 77)
AC_LANG_COMPILER

: > conftest.f
if AC_TRY_COMMAND([$F77 --version | grep GNU >&2]) \
   || AC_TRY_COMMAND([$F77 -v -c conftest.f 2>&1 | grep "f2c " >&2]); then
  # Be sure to remove files which might be created by compilers that
  # don't support --version, or by the second compile.
  rm -f a.exe a.out conftest.f conftest.$ac_objext
  # Has GNU in --version.
  test "$G77" != yes &&
    AC_MSG_ERROR([failed to recognize GNU Fortran 77 compiler])
else
  # Be sure to remove files which might be created by compilers that
  # don't support --version, or by the second compile.
  rm -f a.exe a.out conftest.f conftest.$ac_objext
  # Has not.
  test "$G77" = yes &&
    AC_MSG_ERROR([incorrectly recognized a GNU Fortran 77 compiler])
fi

AC_OUTPUT
AC_STATE_SAVE(after)
_ATEOF

cp "$abs_top_srcdir/build-aux/install-sh" \
   "$abs_top_srcdir/build-aux/config.guess" \
   "$abs_top_srcdir/build-aux/config.sub" .
cp "$abs_top_srcdir/tests/statesave.m4" aclocal.m4


{ set +x
$as_echo "$at_srcdir/fortran.at:31: autoconf --force -W obsolete"
at_fn_check_prepare_trace "fortran.at:31"
( $at_check_trace; autoconf --force -W obsolete
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "fortran.at:31" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/fortran.at:31"
{ set +x
$as_echo "$at_srcdir/fortran.at:31: /bin/sh -n configure"
at_fn_check_prepare_trace "fortran.at:31"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

{ set +x
$as_echo "$at_srcdir/fortran.at:31: autoheader "
at_fn_check_prepare_trace "fortran.at:31"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure
$at_traceon; }



for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/fortran.at:31: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "fortran.at:31"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/fortran.at:31: at_check_env"
at_fn_check_prepare_trace "fortran.at:31"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done

{ set +x
$as_echo "$at_srcdir/fortran.at:31: \$at_diff \"config-h.r1\" \"config-h.r2\""
at_fn_check_prepare_dynamic "$at_diff \"config-h.r1\" \"config-h.r2\"" "fortran.at:31"
( $at_check_trace; $at_diff "config-h.r1" "config-h.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


for act_file in state-env.r1 state-env.r2
do
  $SED '/^ac_cv_/ b
	/^[abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_][abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_0123456789]*=./ !d
	/^[^=]*='\'''\''$/ d
	/^a[cs]_/ d
	/^OLDPWD=/ d
	/^PPID=/ d
	/^RANDOM=/ d
	/^SECONDS=/ d
	/'\'\\\$\''=/ d
	/^argv=/ d
	/^ARGC=/ d
	' $act_file >at_config_vars-$act_file
done
{ set +x
$as_echo "$at_srcdir/fortran.at:31: \$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\""
at_fn_check_prepare_dynamic "$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\"" "fortran.at:31"
( $at_check_trace; $at_diff "at_config_vars-state-env.r1" "at_config_vars-state-env.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:31"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }





  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
