# Check both awk and the result of AC_PROG_AWK
# Quote the first word in the for list for Solaris sh.
for awk_arg in "FOO=" AWK=awk; do
   { set +x
$as_echo "$at_srcdir/torture.at:953: ./configure \$configure_options \$awk_arg"
at_fn_check_prepare_dynamic "./configure $configure_options $awk_arg" "torture.at:953"
( $at_check_trace; ./configure $configure_options $awk_arg
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:953"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:954: cat Foo"
at_fn_check_prepare_trace "torture.at:954"
( $at_check_trace; cat Foo
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "X*'[]+ \", & &\`\\(\$foo \\& \\\\& \\\\\\& \\\\\\\\& \\ \\\\ \\\\\\ !
@foo@ @baz@@notsubsted@bla stray @ and more@@bla
abc@foo@ @baz@baz@baz
abc@foo@ @baz@blabaz
abc@foo@ @baz@blabaz@
abc@bar blabaz
abc@bar blabaz@
abc@bar blabla
@foo@@bar@
@foo@@bar@
X@file@
@file@X
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:954"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:967: cmp allowed-chars Zardoz"
at_fn_check_prepare_trace "torture.at:967"
( $at_check_trace; cmp allowed-chars Zardoz
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:967"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:968: sed '/#/!d
/INTTYPES/d
/MEMORY/d
/PACKAGE_/d
/STDC_HEADERS/d
/STDINT/d
/STDLIB/d
/STRING/d
/SYS_STAT/d
/SYS_TYPES/d
/UNISTD/d' config.h"
at_fn_check_prepare_notrace 'an embedded newline' "torture.at:968"
( $at_check_trace; sed '/#/!d
/INTTYPES/d
/MEMORY/d
/PACKAGE_/d
/STDC_HEADERS/d
/STDINT/d
/STDLIB/d
/STRING/d
/SYS_STAT/d
/SYS_TYPES/d
/UNISTD/d' config.h
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "#define bar %!_!# X
#define barq %!_!# X
#define foo X*'[]+ \", & &\`\\(\$foo !
#define fooq X*'[]+ \", & &\`\\(\$foo !
#define unq1 bla
#define unq2 \$baz
#define unq3 \"bla\"
#define unq4 set
#define unq5 \"a  b\"
#define unq6 hi
#define unq7 '\\\"'
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:968"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done
  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
