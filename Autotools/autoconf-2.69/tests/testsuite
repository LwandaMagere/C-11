  $as_echo "77. $at_setup_line: testing $at_desc ..."
  $at_traceon



# We cannot unset LINENO with Zsh, yet this test case relies on
# unsetting LINENO to compare its result when (i) LINENO is supported
# and when (ii) it is not.
# So just skip if the shell is ZSH.
{ set +x
$as_echo "$at_srcdir/m4sh.at:217: test -n \"\${ZSH_VERSION+set}\" && exit 77"
at_fn_check_prepare_notrace 'a ${...} parameter expansion' "m4sh.at:217"
( $at_check_trace; test -n "${ZSH_VERSION+set}" && exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_skip $at_status "$at_srcdir/m4sh.at:217"
$at_failed && at_fn_log_failure
$at_traceon; }


# AT_DATA_LINENO(FILE-NAME,
#                UNSET-LINENO = true | false, COUNTER, COUNTER-RE)
# ----------------------------------------------------------------
# Produce the FILE-NAME M4sh script which uses the COUNTER LINENO or
# _oline_, which we can recognized via COUNTER-RE.  Unset LINENO is
# UNSET-LINENO.
#
# Use COUNTER, COUNTER-RE = [__LINENO__], [LINENO]
#  or                     = [__OLINE__],  [_oline__]
#
# instead of the obvious $LINENO and 12289, because they would
# be replaced in the test suite itself, even before creating these
# scripts.  For the same reason, grep for LINENO and _oline__ (sic).
#
# UNSET-LINENO is a shell condition to make sure the scripts have the
# same number of lines in the output, so that their outputs be identical.
# AT_DATA_LINENO

# `_oline_', once processed and ran, produces our reference.
# We check that we find ourselves by looking at a string which is
# available only in the original script: `_oline_'.
cat >reference.tas <<'_ATEOF'
AS_INIT
m4_divert_text([], [
if false; then
  AS_UNSET([LINENO])
fi
])
AS_LINENO_PREPARE
echo "Line: __OLINE__"
grep 'Line: .*_oline__' "$[0]" >/dev/null ||
  AS_ERROR([cannot find original script])
exit 0
_ATEOF

# If occurrences of $LINENO or __oline__ were wanted, create them.
sed 's/__LINENO__/$''LINENO/g;s/__OLINE__/__''oline__/g' reference.tas >reference.as
{ set +x
$as_echo "$at_srcdir/m4sh.at:257: autom4te -l m4sh reference.as -o reference"
at_fn_check_prepare_trace "m4sh.at:257"
( $at_check_trace; autom4te -l m4sh reference.as -o reference
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/m4sh.at:257"
$at_failed && at_fn_log_failure
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/m4sh.at:258: ./reference"
at_fn_check_prepare_trace "m4sh.at:258"
( $at_check_trace; ./reference
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
