$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


# config.site can specify a site-wide cache, accumulating information.
# Also test that we don't run afoul of sourcing a file with leading -.
cat >-config.site <<'_ATEOF'
cache_file=sitecache
_ATEOF

cat >sitecache <<'_ATEOF'
my_cv_some_preset_cache_var=yes
_ATEOF

CONFIG_SITE=-config.site
export CONFIG_SITE
 { set +x
$as_echo "$at_srcdir/base.at:479: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "base.at:479"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
