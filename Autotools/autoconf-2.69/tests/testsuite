	/^[abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_][abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_0123456789]*=./ !d
	/^[^=]*='\'''\''$/ d
	/^a[cs]_/ d
	/^OLDPWD=/ d
	/^PPID=/ d
	/^RANDOM=/ d
	/^SECONDS=/ d
	/'\'\\\$\''=/ d
	/^argv=/ d
	/^ARGC=/ d
	' $act_file >at_config_vars-$act_file
done
{ set +x
$as_echo "$at_srcdir/torture.at:35: \$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\""
at_fn_check_prepare_dynamic "$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\"" "torture.at:35"
( $at_check_trace; $at_diff "at_config_vars-state-env.r1" "at_config_vars-state-env.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:35"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }




{ set +x
$as_echo "$at_srcdir/torture.at:35: test ! -f tmpdir"
at_fn_check_prepare_trace "torture.at:35"
( $at_check_trace; test ! -f tmpdir
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:35"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:35: ACTION=use ./config.status"
at_fn_check_prepare_trace "torture.at:35"
( $at_check_trace; ACTION=use ./config.status
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:35"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:35: test -s tmpdir && test ! -d \"\`cat tmpdir\`\""
at_fn_check_prepare_notrace 'a `...` command substitution' "torture.at:35"
( $at_check_trace; test -s tmpdir && test ! -d "`cat tmpdir`"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:35"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:35: ACTION=corrupt ./config.status"
at_fn_check_prepare_trace "torture.at:35"
( $at_check_trace; ACTION=corrupt ./config.status
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:35"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:35: test -s tmpdir && test ! -d \"\`cat tmpdir\`\""
at_fn_check_prepare_notrace 'a `...` command substitution' "torture.at:35"
( $at_check_trace; test -s tmpdir && test ! -d "`cat tmpdir`"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
