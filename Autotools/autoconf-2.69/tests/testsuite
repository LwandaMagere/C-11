{ set +x
$as_echo "$at_srcdir/m4sugar.at:597: sed 's/^[^:]*m4[-.ex0-9]*: *\\([^:]*:\\) *\\([0-9][0-9]*: \\)/m4:\\1\\2/
	s/^\\([^:]*:\\) *\\([0-9][0-9]*:\\)[^:]*m4[-.ex0-9]*: /m4:\\1\\2 /
	s/: C\\(annot open \\)\\([^\`:]*\\):/: c\\1\`\\2'\\'':/
	s/: include:\\( cannot open\\)/:\\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)\$//
    ' stderr >&2"
at_fn_check_prepare_notrace 'a `...` command substitution' "m4sugar.at:597"
( $at_check_trace; sed 's/^[^:]*m4[-.ex0-9]*: *\([^:]*:\) *\([0-9][0-9]*: \)/m4:\1\2/
	s/^\([^:]*:\) *\([0-9][0-9]*:\)[^:]*m4[-.ex0-9]*: /m4:\1\2 /
	s/: C\(annot open \)\([^`:]*\):/: c\1`\2'\'':/
	s/: include:\( cannot open\)/:\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)$//
    ' stderr >&2
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo >>"$at_stderr"; $as_echo "script.4s:15: warning: m4_require: \`a' was expanded before it was required
script.4s:15: http://www.gnu.org/software/autoconf/manual/autoconf.html#Expanded-Before-Required
script.4s:5: b is expanded from...
script.4s:6: c is expanded from...
script.4s:14: outer is expanded from...
script.4s:15: the top level
" | \
  $at_diff - "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/m4sugar.at:597"
$at_failed && at_fn_log_failure
$at_traceon; }





