# Check that no use of `ac_write_fail' escaped into config.status
{ set +x
$as_echo "$at_srcdir/torture.at:332: grep ac_write_fail config.status"
at_fn_check_prepare_trace "torture.at:332"
( $at_check_trace; grep ac_write_fail config.status
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:332"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


# Check that --file and --header accept funny file names
cat >fgrep.in <<'_ATEOF'
FGREP="@FGREP@"
_ATEOF

./config.status --file=fgrep:fgrep.in
. ./fgrep

x=
export x
for file in \
  'with  funny '\'' $x & #! name' \
  'file with  funny \ '\'' \'\'' $ & #!*? name' \
  'with  funny \ '\'' \'\'' " | <a >b & * ? name ' # "restore font-lock
do
  # The function func_sanitize_file_name comes from tools.at
  file=`func_sanitize_file_name "$file"`
  cat >"$file.in" <<'END'
@configure_input@
END
  { set +x
$as_echo "$at_srcdir/torture.at:353: ./config.status \"--file=\$file:\$file.in\""
at_fn_check_prepare_dynamic "./config.status \"--file=$file:$file.in\"" "torture.at:353"
( $at_check_trace; ./config.status "--file=$file:$file.in"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:353"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:355: \$FGREP \"\$file\" \"\$file\""
at_fn_check_prepare_dynamic "$FGREP \"$file\" \"$file\"" "torture.at:355"
( $at_check_trace; $FGREP "$file" "$file"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:355"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:356: ./config.status \"--header=\$file:\$file.in\""
at_fn_check_prepare_dynamic "./config.status \"--header=$file:$file.in\"" "torture.at:356"
( $at_check_trace; ./config.status "--header=$file:$file.in"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:356"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  # Run the same test a 2nd time to see that config.status does not recreate
  # the header (regression test)
  { set +x
$as_echo "$at_srcdir/torture.at:360: ./config.status \"--header=\$file:\$file.in\""
at_fn_check_prepare_dynamic "./config.status \"--header=$file:$file.in\"" "torture.at:360"
( $at_check_trace; ./config.status "--header=$file:$file.in"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "config.status: creating $file
config.status: $file is unchanged
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:360"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:364: grep ' & ' \"\$file\""
at_fn_check_prepare_dynamic "grep ' & ' \"$file\"" "torture.at:364"
( $at_check_trace; grep ' & ' "$file"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "/* $file.  Generated from $file.in by configure.  */
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:364"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

  { set +x
$as_echo "$at_srcdir/torture.at:367: \$FGREP \"\$file\" \"\$file\""
at_fn_check_prepare_dynamic "$FGREP \"$file\" \"$file\"" "torture.at:367"
( $at_check_trace; $FGREP "$file" "$file"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:367"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done
  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
