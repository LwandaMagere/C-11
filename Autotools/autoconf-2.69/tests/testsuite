    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/semantics.at:77: at_check_env"
at_fn_check_prepare_trace "semantics.at:77"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:77"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done

{ set +x
$as_echo "$at_srcdir/semantics.at:77: \$at_diff \"config-h.r1\" \"config-h.r2\""
at_fn_check_prepare_dynamic "$at_diff \"config-h.r1\" \"config-h.r2\"" "semantics.at:77"
( $at_check_trace; $at_diff "config-h.r1" "config-h.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
