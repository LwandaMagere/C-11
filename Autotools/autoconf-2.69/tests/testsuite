$at_failed && at_fn_log_failure
$at_traceon; }

fi

cp "$abs_top_srcdir/build-aux/install-sh" build-aux
 { set +x
$as_echo "$at_srcdir/foreign.at:129: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "foreign.at:129"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:129"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/foreign.at:130: \$MAKE copy-shtool"
at_fn_check_prepare_dynamic "$MAKE copy-shtool" "foreign.at:130"
( $at_check_trace; $MAKE copy-shtool
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:130"
if $at_failed; then :
  { set +x
$as_echo "$at_srcdir/foreign.at:130: : >build-aux/shtool"
at_fn_check_prepare_trace "foreign.at:130"
( $at_check_trace; : >build-aux/shtool
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:130"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

fi
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

rm -f build-aux/install-sh
 { set +x
$as_echo "$at_srcdir/foreign.at:133: ./configure \$configure_options --prefix=\"\$instdir\" ac_cv_path_SHTOOL=false"
at_fn_check_prepare_dynamic "./configure $configure_options --prefix=\"$instdir\" ac_cv_path_SHTOOL=false" "foreign.at:133"
( $at_check_trace; ./configure $configure_options --prefix="$instdir" ac_cv_path_SHTOOL=false
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:133"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/foreign.at:134: grep '^ac_install_sh = .*shtool install -c' Makefile"
at_fn_check_prepare_trace "foreign.at:134"
( $at_check_trace; grep '^ac_install_sh = .*shtool install -c' Makefile
