mkdir serial

# Unfortunately, the return value of wait is unreliable,
# so we check that kill fails.
{ set +x
$as_echo "$at_srcdir/autotest.at:1451: \$CONFIG_SHELL ./micro-suite --jobs=4 &          sleep 8 &&          cd serial && \$CONFIG_SHELL ../micro-suite -4 >/dev/null &&          { kill \$! && exit 1; :; }"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite --jobs=4 &          sleep 8 &&          cd serial && $CONFIG_SHELL ../micro-suite -4 >/dev/null &&          { kill $! && exit 1; :; }" "autotest.at:1451"
( $at_check_trace; $CONFIG_SHELL ./micro-suite --jobs=4 &          sleep 8 &&          cd serial && $CONFIG_SHELL ../micro-suite -4 >/dev/null &&          { kill $! && exit 1; :; }
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1451"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1455: grep -c '^.\\{53\\}ok' stdout"
