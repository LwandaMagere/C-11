$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:309"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
