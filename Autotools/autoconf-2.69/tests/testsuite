at_fn_check_status 0 $at_status "$at_srcdir/c.at:248"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/c.at:248: at_check_env"
at_fn_check_prepare_trace "c.at:248"
