$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

# Force write error creating a file on stdout
if test -w /dev/full && test -c /dev/full; then
  { set +x
$as_echo "$at_srcdir/torture.at:302: ./config.status --file=-:input </dev/null >/dev/full || exit 1"
at_fn_check_prepare_trace "torture.at:302"
( $at_check_trace; ./config.status --file=-:input </dev/null >/dev/full || exit 1
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/torture.at:302"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

fi

# Validate that --file requires an argument
{ set +x
$as_echo "$at_srcdir/torture.at:307: ./config.status --file"
at_fn_check_prepare_trace "torture.at:307"
( $at_check_trace; ./config.status --file
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
