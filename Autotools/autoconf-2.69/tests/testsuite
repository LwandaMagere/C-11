# Ensure that m4sugar dies when dereferencing undefined macros, whether
# this is provided by m4 natively or faked by wrappers in m4sugar.

cat >script.4s <<'_ATEOF'
m4_define([good])
m4_defn([good], [oops])
_ATEOF



{ set +x
$as_echo "$at_srcdir/m4sugar.at:115: autom4te --language=m4sugar script.4s -o script -o-"
at_fn_check_prepare_trace "m4sugar.at:115"
( $at_check_trace; autom4te --language=m4sugar script.4s -o script -o-
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/m4sugar.at:115"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/m4sugar.at:115: sed 's/^[^:]*m4[-.ex0-9]*: *\\([^:]*:\\) *\\([0-9][0-9]*: \\)/m4:\\1\\2/
	s/^\\([^:]*:\\) *\\([0-9][0-9]*:\\)[^:]*m4[-.ex0-9]*: /m4:\\1\\2 /
	s/: C\\(annot open \\)\\([^\`:]*\\):/: c\\1\`\\2'\\'':/
	s/: include:\\( cannot open\\)/:\\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)\$//
    ' stderr >&2"
at_fn_check_prepare_notrace 'a `...` command substitution' "m4sugar.at:115"
( $at_check_trace; sed 's/^[^:]*m4[-.ex0-9]*: *\([^:]*:\) *\([0-9][0-9]*: \)/m4:\1\2/
	s/^\([^:]*:\) *\([0-9][0-9]*:\)[^:]*m4[-.ex0-9]*: /m4:\1\2 /
	s/: C\(annot open \)\([^`:]*\):/: c\1`\2'\'':/
	s/: include:\( cannot open\)/:\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)$//
    ' stderr >&2
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/m4sugar.at:115"
$at_failed && at_fn_log_failure
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/m4sugar.at:116: grep good stderr"
at_fn_check_prepare_trace "m4sugar.at:116"
( $at_check_trace; grep good stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/m4sugar.at:116"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/m4sugar.at:117: grep 'm4_defn: undefined.*oops' stderr"
at_fn_check_prepare_trace "m4sugar.at:117"
( $at_check_trace; grep 'm4_defn: undefined.*oops' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
