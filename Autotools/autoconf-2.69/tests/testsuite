cat >configure.ac <<'_ATEOF'
AC_INIT
AC_CONFIG_HEADERS(config.h:config.hin)
AC_STATE_SAVE(before)
AC_C_BIGENDIAN(
      [ac_endian=big],
      [ac_endian=little],
      [ac_endian=unknown],
      [ac_endian=universal])
   echo $ac_endian > at-endian

AC_OUTPUT
AC_STATE_SAVE(after)
_ATEOF

