for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/semantics.at:744: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "semantics.at:744"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
