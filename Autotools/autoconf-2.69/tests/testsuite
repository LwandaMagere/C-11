429;acprograms.at:26;AC_DECL_YYTEXT;autoupdate;
430;acprograms.at:27;AC_RSH;autoupdate;
431;acheaders.at:13;AC_CHECK_HEADERS_ONCE;;
432;acheaders.at:14;AC_CHECK_HEADER_STDBOOL;;
433;acheaders.at:15;AC_HEADER_ASSERT;;
434;acheaders.at:16;AC_HEADER_MAJOR;;
