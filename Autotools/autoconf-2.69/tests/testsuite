$at_failed && at_fn_log_failure
$at_traceon; }

# Modern Libtool has further tests for compatibility with older autoconf;
# from the Autoconf side, we will merely assume newer libtool.
{ set +x
$as_echo "$at_srcdir/foreign.at:32:
case \`libtoolize --version |
      sed -n '/^.* \\([0-9][0-9.a-z]*\\)\$/{
		s//\\1/
		p
		q
	}'\` in
  0.*    ) exit 77;;
  1.*    ) exit 77;;
esac"
at_fn_check_prepare_notrace 'a `...` command substitution' "foreign.at:32"
( $at_check_trace;
case `libtoolize --version |
      sed -n '/^.* \([0-9][0-9.a-z]*\)$/{
		s//\1/
		p
		q
	}'` in
  0.*    ) exit 77;;
  1.*    ) exit 77;;
esac
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_skip $at_status "$at_srcdir/foreign.at:32"
$at_failed && at_fn_log_failure
$at_traceon; }


# Using a configure.ac, have libtoolize confess where libtool.m4 is.
cat >configure.ac <<'_ATEOF'
AC_INIT
# State that we explicitly want auxiliary files here, so libtoolize
# won't pollute files outside the test directory.
AC_CONFIG_AUX_DIR([.])
AC_PROG_LIBTOOL
_ATEOF

{ set +x
