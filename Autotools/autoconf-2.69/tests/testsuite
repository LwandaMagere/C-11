cat >test/test-1.tas <<'_ATEOF'
AS_INIT
m4_divert_text([], [
if false; then
  AS_UNSET([LINENO])
fi
])
AS_LINENO_PREPARE
echo "Line: __LINENO__"
grep 'Line: .*LINENO' "$[0]" >/dev/null ||
  AS_ERROR([cannot find original script])
exit 0
_ATEOF

# If occurrences of $LINENO or __oline__ were wanted, create them.
