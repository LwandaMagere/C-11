$as_echo "$at_srcdir/torture.at:133:
if (FOO=FOO; unset FOO) >/dev/null 2>&1; then
  exit 0
else
  exit 77
fi
"
at_fn_check_prepare_notrace 'an embedded newline' "torture.at:133"
