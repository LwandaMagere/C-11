at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:343"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


for act_file in state-env.r1 state-env.r2
do
  $SED '/^ac_cv_/ b
	/^[abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_][abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_0123456789]*=./ !d
	/^[^=]*='\'''\''$/ d
	/^a[cs]_/ d
	/^OLDPWD=/ d
	/^PPID=/ d
	/^RANDOM=/ d
	/^SECONDS=/ d
	/'\'\\\$\''=/ d
	/^argv=/ d
	/^ARGC=/ d
	' $act_file >at_config_vars-$act_file
done
{ set +x
$as_echo "$at_srcdir/semantics.at:343: \$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\""
at_fn_check_prepare_dynamic "$at_diff \"at_config_vars-state-env.r1\" \"at_config_vars-state-env.r2\"" "semantics.at:343"
( $at_check_trace; $at_diff "at_config_vars-state-env.r1" "at_config_vars-state-env.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:343"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }



{ set +x
$as_echo "$at_srcdir/semantics.at:343: sed '/#/!d
/INTTYPES/d
/MEMORY/d
/PACKAGE_/d
/STDC_HEADERS/d
/STDINT/d
/STDLIB/d
/STRING/d
/SYS_STAT/d
/SYS_TYPES/d
/UNISTD/d' config.h"
at_fn_check_prepare_notrace 'an embedded newline' "semantics.at:343"
( $at_check_trace; sed '/#/!d
/INTTYPES/d
/MEMORY/d
/PACKAGE_/d
/STDC_HEADERS/d
/STDINT/d
/STDLIB/d
/STRING/d
/SYS_STAT/d
/SYS_TYPES/d
/UNISTD/d' config.h
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "#define ALIGNOF_CHAR 1
#define ALIGNOF_CHARCHAR 1
#define ALIGNOF_CHARCHARCHAR 0
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:343"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
#AT_STOP_364
#AT_START_365
at_fn_group_banner 365 'semantics.at:343' \
  "AC_CHECK_ALIGNOF (cross compile)" "               " 17
at_xfail=no
(
  $as_echo "365. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >configure.ac <<'_ATEOF'
AC_INIT
AC_CONFIG_HEADERS(config.h:config.hin)
AC_STATE_SAVE(before)

  # Exercise the code used when cross-compiling.
  cross_compiling=yes
  ac_tool_warned=yes
  AC_CHECK_ALIGNOF(char)
AC_CHECK_ALIGNOF(charchar,
[[#include <stddef.h>
#include <stdio.h>
typedef char charchar[2];]])
AC_CHECK_ALIGNOF(charcharchar)

AC_OUTPUT
AC_STATE_SAVE(after)
_ATEOF

