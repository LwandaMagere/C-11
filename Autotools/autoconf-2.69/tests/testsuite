  $as_echo "erlang.at:52" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/erlang.at:52"
{ set +x
$as_echo "$at_srcdir/erlang.at:52: /bin/sh -n configure"
at_fn_check_prepare_trace "erlang.at:52"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
