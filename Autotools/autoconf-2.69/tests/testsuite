at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:343"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

{ set +x
$as_echo "$at_srcdir/semantics.at:343: autoheader "
at_fn_check_prepare_trace "semantics.at:343"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:343"
$at_failed && at_fn_log_failure
$at_traceon; }



for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/semantics.at:343: ./configure \$configure_options -C"
