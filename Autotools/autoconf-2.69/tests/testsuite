at_fn_check_status 0 $at_status "$at_srcdir/go.at:30"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/go.at:30: at_check_env"
