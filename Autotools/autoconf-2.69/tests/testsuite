417;acspecific.at:28;AC_XENIX_DIR;autoupdate;
418;acprograms.at:13;AC_PROG_AWK;;
419;acprograms.at:14;AC_PROG_FGREP;;
420;acprograms.at:15;AC_PROG_INSTALL;;
