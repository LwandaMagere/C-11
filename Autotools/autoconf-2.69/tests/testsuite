AT_INIT([artificial test suite])
AT_SETUP([my only test])
AT_CHECK([test ! -f cleanup.success && test ! -f cleanup.failure])
   AT_XFAIL_IF([$xfail])
   AT_CHECK_UNQUOTED([exit $value], [ignore], [$output],
     [], [touch cleanup.failure], [touch cleanup.success])
AT_CLEANUP
_ATEOF

