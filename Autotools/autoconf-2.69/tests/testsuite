$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }






{ set +x
$as_echo "$at_srcdir/autotest.at:837: \$CONFIG_SHELL ./micro-suite "
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite " "autotest.at:837"
( $at_check_trace; $CONFIG_SHELL ./micro-suite
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:837"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:837: \$CONFIG_SHELL ./micro-suite -v -x "
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite -v -x " "autotest.at:837"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -v -x
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:837"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


{ set +x
