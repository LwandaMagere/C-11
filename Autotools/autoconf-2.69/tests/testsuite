at_fn_check_status 1 $at_status "$at_srcdir/autotest.at:1652"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

for color in "$red" "$grn" "$lgn" "$blu"; do
  { set +x
