at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1427"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }


# Per BUGS, we have not yet figured out how to run parallel tests cleanly
# under dash and some ksh variants.  For now, only run this test under
# limited conditions; help is appreciated in widening this test base.
$as_echo "autotest.at:1429" >"$at_check_line_file"
(${CONFIG_SHELL-$SHELL} -c 'test -z "${BASH_VERSION+set}${ZSH_VERSION+set}${TEST_PARALLEL_AUTOTEST+set}"') \
  && at_fn_check_skip 77 "$at_srcdir/autotest.at:1429"
# The parallel scheduler requires mkfifo and job control to work.
{ set +x
$as_echo "$at_srcdir/autotest.at:1429: mkfifo fifo || exit 77"
at_fn_check_prepare_trace "autotest.at:1429"
( $at_check_trace; mkfifo fifo || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1429"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1429: \${CONFIG_SHELL-\$SHELL} -c '(set -m && set +m) || exit 77'"
at_fn_check_prepare_notrace 'a ${...} parameter expansion' "autotest.at:1429"
( $at_check_trace; ${CONFIG_SHELL-$SHELL} -c '(set -m && set +m) || exit 77'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1429"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }



# Ensure that all tests run, and lines are not split.
{ set +x
$as_echo "$at_srcdir/autotest.at:1432: \$CONFIG_SHELL ./micro-suite -j4"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite -j4" "autotest.at:1432"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -j4
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1432"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1433: grep -c '^.\\{53\\}ok' stdout"
at_fn_check_prepare_trace "autotest.at:1433"
( $at_check_trace; grep -c '^.\{53\}ok' stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "16
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1433"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

# Running one test with -j should produce correctly formatted output:
{ set +x
$as_echo "$at_srcdir/autotest.at:1436: \$CONFIG_SHELL ./micro-suite -j 3"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite -j 3" "autotest.at:1436"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -j 3
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1436"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:1437: grep -c '^.\\{53\\}ok' stdout"
at_fn_check_prepare_trace "autotest.at:1437"
( $at_check_trace; grep -c '^.\{53\}ok' stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "1
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:1437"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

# Specifying more jobs than tests should not hang:
{ set +x
$as_echo "$at_srcdir/autotest.at:1440: \$CONFIG_SHELL ./micro-suite -j3 3"
at_fn_check_prepare_dynamic "$CONFIG_SHELL ./micro-suite -j3 3" "autotest.at:1440"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -j3 3
