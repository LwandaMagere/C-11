at_fn_check_status 0 $at_status "$at_srcdir/erlang.at:30"
$at_failed && at_fn_log_failure
$at_traceon; }



for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/erlang.at:30: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "erlang.at:30"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/erlang.at:30"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/erlang.at:30: at_check_env"
at_fn_check_prepare_trace "erlang.at:30"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/erlang.at:30"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done

{ set +x
$as_echo "$at_srcdir/erlang.at:30: \$at_diff \"config-h.r1\" \"config-h.r2\""
at_fn_check_prepare_dynamic "$at_diff \"config-h.r1\" \"config-h.r2\"" "erlang.at:30"
( $at_check_trace; $at_diff "config-h.r1" "config-h.r2"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
