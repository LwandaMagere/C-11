# Make sure at-path contains something valid, and let the test suite
# display it when verbose.  And fail, skipping would too easily hide
# problems.
{ set +x
$as_echo "$at_srcdir/foreign.at:60: sed -n \"s,^.*\\\`\\\\(/[^']*\\\\)'.*,\\\\1,p\" stdout"
at_fn_check_prepare_notrace 'a `...` command substitution' "foreign.at:60"
( $at_check_trace; sed -n "s,^.*\`\\(/[^']*\\)'.*,\\1,p" stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:60"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/foreign.at:61: test -f \"\`sed -n 1p stdout\`\""
at_fn_check_prepare_notrace 'a `...` command substitution' "foreign.at:61"
