{ set +x
$as_echo "$at_srcdir/tools.at:1238: sed 's/^[^:]*m4[-.ex0-9]*: *\\([^:]*:\\) *\\([0-9][0-9]*: \\)/m4:\\1\\2/
	s/^\\([^:]*:\\) *\\([0-9][0-9]*:\\)[^:]*m4[-.ex0-9]*: /m4:\\1\\2 /
	s/: C\\(annot open \\)\\([^\`:]*\\):/: c\\1\`\\2'\\'':/
	s/: include:\\( cannot open\\)/:\\1/
	s/^autom4te: [^ ]*m4[.ex]* /autom4te: m4 /
	s/ (E[A-Z]*)\$//
    ' stderr >&2"
at_fn_check_prepare_notrace 'a `...` command substitution' "tools.at:1238"
( $at_check_trace; sed 's/^[^:]*m4[-.ex0-9]*: *\([^:]*:\) *\([0-9][0-9]*: \)/m4:\1\2/
	s/^\([^:]*:\) *\([0-9][0-9]*:\)[^:]*m4[-.ex0-9]*: /m4:\1\2 /
	s/: C\(annot open \)\([^`:]*\):/: c\1`\2'\'':/
