at_fn_check_prepare_notrace 'an embedded newline' "semantics.at:732"
( $at_check_trace; sed '/#/!d
/INTTYPES/d
/MEMORY/d
/PACKAGE_/d
/STDC_HEADERS/d
/STDINT/d
/STDLIB/d
