at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/acc.at:29"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

done

{ set +x
