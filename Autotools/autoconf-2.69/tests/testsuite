$as_echo "$at_srcdir/semantics.at:388: autoconf --force -W obsolete"
at_fn_check_prepare_trace "semantics.at:388"
( $at_check_trace; autoconf --force -W obsolete
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/semantics.at:388"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "semantics.at:388" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/semantics.at:388"
{ set +x
$as_echo "$at_srcdir/semantics.at:388: /bin/sh -n configure"
at_fn_check_prepare_trace "semantics.at:388"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
