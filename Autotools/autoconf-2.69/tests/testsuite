#AT_STOP_322
#AT_START_323
at_fn_group_banner 323 'fortran.at:973' \
  "AC_FC_LINE_LENGTH" "                              " 12
at_xfail=no
(
  $as_echo "323. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >Makefile.in <<'_ATEOF'
prog@EXEEXT@: prog.@OBJEXT@
	@FC@ @FCFLAGS@ -o $@ prog.@OBJEXT@ @LIBS@

.SUFFIXES: .f .@OBJEXT@
.f.@OBJEXT@:
	@FC@ @FCFLAGS@ -c @FCFLAGS_f@ $<

clean:
	rm -f *.@OBJEXT@ prog@EEXEXT@
_ATEOF


line_80=\
'subroutine foo(arg1,arg2,arg3,arg4,arg5,arg6,arg7,arg8,arg9,arg10,arg11)'
line_132=\
'subroutine foo(arg1,arg2,arg3,arg4,arg5,arg6,arg7,arg8,arg9,arg10,arg11,'\
'arg12,arg13,arg14,arg15,arg16,arg17,arg18,arg19)'
line_254=\
'subroutine foo(arg1,arg2,arg3,arg4,arg5,arg6,arg7,arg8,arg9,arg10,arg11,'\
'arg12,arg13,arg14,arg15,arg16,arg17,arg18,arg19,arg20,arg21,arg22,arg23,'\
'arg24,arg25,arg26,arg27,arg28,arg29,arg30,arg31,arg32,arg33,arg34,arg35,'\
'arg36,arg37,arg38,arg39,arg40)'

for len in 80 132 254
do
  if test $len -eq 254; then arg=unlimited; else arg=$len; fi
  eval long_line=\$line_$len

  # Try free-form first, it has a bigger chance of succeeding.
  for fixed_or_free in FREEFORM FIXEDFORM
  do

    cat >configure.ac <<EOF
AC_INIT
AC_PROG_FC
AC_FC_SRCEXT(f)
AC_FC_$fixed_or_free
AC_FC_LINE_LENGTH($arg)
AC_CONFIG_FILES(Makefile)
AC_OUTPUT
EOF

    cat >prog.f <<EOF
       $long_line
       end subroutine
       program main
       end program
EOF

    { set +x
$as_echo "$at_srcdir/fortran.at:1024: autoconf --force "
at_fn_check_prepare_trace "fortran.at:1024"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:1024"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "fortran.at:1024" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/fortran.at:1024"
{ set +x
$as_echo "$at_srcdir/fortran.at:1024: /bin/sh -n configure"
at_fn_check_prepare_trace "fortran.at:1024"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:1024"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

     { set +x
$as_echo "$at_srcdir/fortran.at:1025: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "fortran.at:1025"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:1025"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    : "${MAKE=make}"
    { set +x
$as_echo "$at_srcdir/fortran.at:1027: \$MAKE"
at_fn_check_prepare_dynamic "$MAKE" "fortran.at:1027"
( $at_check_trace; $MAKE
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:1027"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

        { set +x
$as_echo "$at_srcdir/fortran.at:1029: \$MAKE clean"
at_fn_check_prepare_dynamic "$MAKE clean" "fortran.at:1029"
( $at_check_trace; $MAKE clean
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/fortran.at:1029"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


  done
done

  set +x
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 7>&- | eval $at_tee_pipe
read at_status <"$at_status_file"
