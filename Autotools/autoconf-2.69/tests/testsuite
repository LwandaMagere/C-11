# Skip this test if there is no libtoolize.
{ set +x
$as_echo "$at_srcdir/foreign.at:28: libtoolize --version || exit 77"
at_fn_check_prepare_trace "foreign.at:28"
( $at_check_trace; libtoolize --version || exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
