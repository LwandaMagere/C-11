# Check that config cache scripts must be well-formed.
cat >bad.site <<'_ATEOF'
fi
_ATEOF

CONFIG_SITE=$PWD/bad.site
 { set +x
$as_echo "$at_srcdir/base.at:496: ./configure \$configure_options  || exit 1"
at_fn_check_prepare_dynamic "./configure $configure_options  || exit 1" "base.at:496"
( $at_check_trace; ./configure $configure_options  || exit 1
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; tee stderr <"$at_stderr"
echo stdout:; tee stdout <"$at_stdout"
at_fn_check_status 1 $at_status "$at_srcdir/base.at:496"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/base.at:497: grep 'failed to load site script' stderr"
at_fn_check_prepare_trace "base.at:497"
( $at_check_trace; grep 'failed to load site script' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/base.at:497"
if $at_failed; then :
  { set +x
$as_echo "$at_srcdir/base.at:497: grep 'whether true' stdout"
at_fn_check_prepare_trace "base.at:497"
( $at_check_trace; grep 'whether true' stdout
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 1 $at_status "$at_srcdir/base.at:497"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

fi
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


# However, a missing file is ignored.
CONFIG_SITE=./no-such-file
 { set +x
$as_echo "$at_srcdir/base.at:502: ./configure \$configure_options "
