AC_INIT
_ATEOF


cat >.autom4te.cfg <<'_ATEOF'
begin-language: "Autoconf-without-aclocal-m4"
args: --cache=sub/autom4te.cache
end-language: "Autoconf-without-aclocal-m4"
_ATEOF


# Do not try this when we are root or on systems without permissions.
# A failed redirection may cause a status of 2 with FreeBSD sh.
{ set +x
$as_echo "$at_srcdir/tools.at:1235: (: > sub/some-file) || exit 1 && exit 77"
at_fn_check_prepare_trace "tools.at:1235"
( $at_check_trace; (: > sub/some-file) || exit 1 && exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
