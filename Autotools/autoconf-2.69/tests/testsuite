cat >configure.ac <<'_ATEOF'
m4_define([active], [ACTIVE])
m4_define([TRACE1], [TRACE2(m4_shift($@))])
m4_define([TRACE2], [[$2], $1])

# No arguments.
TRACE1
TRACE2

# With arguments, single line.
TRACE1(foo, @bar, @baz)
TRACE1(foo, TRACE1(bar, baz))
TRACE1(foo, active, baz)
TRACE1(foo, [active], TRACE1(active, [active]))
_ATEOF


# Several --traces.
{ set +x
$as_echo "$at_srcdir/tools.at:271: autoconf --force -t TRACE1 -t TRACE2"
at_fn_check_prepare_trace "tools.at:271"
( $at_check_trace; autoconf --force -t TRACE1 -t TRACE2
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "configure.ac:6:TRACE1:
configure.ac:6:TRACE2:
configure.ac:7:TRACE2:
configure.ac:10:TRACE1:foo:@bar:@baz
configure.ac:10:TRACE2:@bar:@baz
configure.ac:11:TRACE1:bar:baz
configure.ac:11:TRACE2:baz
configure.ac:11:TRACE1:foo::baz
configure.ac:11:TRACE2::baz
configure.ac:12:TRACE1:foo:ACTIVE:baz
configure.ac:12:TRACE2:ACTIVE:baz
configure.ac:13:TRACE1:ACTIVE:active
configure.ac:13:TRACE2:active
configure.ac:13:TRACE1:foo:active::ACTIVE
configure.ac:13:TRACE2:active::ACTIVE
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:271"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "tools.at:271" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/tools.at:271"
{ set +x
$as_echo "$at_srcdir/tools.at:271: /bin/sh -n configure"
at_fn_check_prepare_trace "tools.at:271"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:271"
$at_failed && at_fn_log_failure
$at_traceon; }

fi


# Several line requests.
{ set +x
$as_echo "$at_srcdir/tools.at:290: autoconf --force -t TRACE1:'
[\$1], [\$2], [\$3].'"
at_fn_check_prepare_notrace 'an embedded newline' "tools.at:290"
( $at_check_trace; autoconf --force -t TRACE1:'
[$1], [$2], [$3].'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "
[], [], [].

[foo], [@bar], [@baz].

[bar], [baz], [].

[foo], [], [baz].

[foo], [ACTIVE], [baz].

[ACTIVE], [active], [].

[foo], [active], [].
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/tools.at:290"
$at_failed && at_fn_log_failure
$at_traceon; }



