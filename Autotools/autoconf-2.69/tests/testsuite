for at_run in r1 r2
do
     { set +x
$as_echo "$at_srcdir/acc.at:29: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "acc.at:29"
( $at_check_trace; ./configure $configure_options -C
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/acc.at:29"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

    cp -f state-env.after state-env.$at_run
    cp -f config.h config-h.$at_run
    { set +x
$as_echo "$at_srcdir/acc.at:29: at_check_env"
at_fn_check_prepare_trace "acc.at:29"
( $at_check_trace; at_check_env
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
