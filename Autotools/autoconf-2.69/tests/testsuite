fi


# Ignore stderr, because ltconfig always loads the cache, which is
# /dev/null, and some shells choke on this.  For instance with Bash
# 2.05, you get:
#
#   loading cache /dev/null within ltconfig
#   ./ltconfig: .: /dev/null: not a regular file
#
# But the script executes properly.
 { set +x
$as_echo "$at_srcdir/foreign.at:87: ./configure \$configure_options "
at_fn_check_prepare_dynamic "./configure $configure_options " "foreign.at:87"
( $at_check_trace; ./configure $configure_options
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/foreign.at:87"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


  set +x
  $at_times_p && times >"$at_times_file"
