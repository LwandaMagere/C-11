$as_echo "$at_srcdir/base.at:354: grep 'must contain _cv_ to be cached' stderr"
at_fn_check_prepare_trace "base.at:354"
( $at_check_trace; grep 'must contain _cv_ to be cached' stderr
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/base.at:354"
$at_failed && at_fn_log_failure
$at_traceon; }


