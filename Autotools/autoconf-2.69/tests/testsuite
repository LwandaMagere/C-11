# Without options, when all tests pass, no test directory should exist.
{ set +x
$as_echo "$at_srcdir/autotest.at:896: test -d micro-suite.dir/1 && exit 42
          \$CONFIG_SHELL ./micro-suite -d 1"
at_fn_check_prepare_notrace 'an embedded newline' "autotest.at:896"
( $at_check_trace; test -d micro-suite.dir/1 && exit 42
          $CONFIG_SHELL ./micro-suite -d 1
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:896"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

# Running with -d should leave a reproducible test group.
# Also, running the test script from the test group locks the
# directory from removal on some platforms; the script should still be
# able to run even if rmdir fails.
{ set +x
$as_echo "$at_srcdir/autotest.at:896: (cd micro-suite.dir/1 && ./run)"
at_fn_check_prepare_trace "autotest.at:896"
( $at_check_trace; (cd micro-suite.dir/1 && ./run)
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:896"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

# Running a debugging script implies -d.
{ set +x
$as_echo "$at_srcdir/autotest.at:896: (cd micro-suite.dir/1 && ./run)"
at_fn_check_prepare_trace "autotest.at:896"
