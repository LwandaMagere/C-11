  extract_version='s/^[^0-9]*\([0-9][^ ]*\).*/\1/;q'
	  automake_version=`automake --version | sed "$extract_version"`
	  used_automake_version=`sed "$extract_version" "$abs_top_srcdir/Makefile.in"`
	  { set +x
