AC_CONFIG_FILES([sub1/file1 file2])
AC_CONFIG_LINKS([file1:sub1/file1 sub2/file2:file2])
AC_OUTPUT
_ATEOF


mkdir sub1

cat >sub1/file1.in <<'_ATEOF'
/* @configure_input@ */
#define PACKAGE_STRING "@PACKAGE_STRING@"
_ATEOF


cat >file2.in <<'_ATEOF'
/* @configure_input@ */
#define PACKAGE_STRING "@PACKAGE_STRING@"
_ATEOF


