) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "A # in my name
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/autotest.at:839"
$at_failed && at_fn_log_failure  \
"./micro-suite.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/autotest.at:839: \$CONFIG_SHELL ./micro-suite -l |
	   sed -n 's/.*[0-9]: [^	 ][^	 ]*[	 ][	 ]*\\(.*[^	 ]\\)[	 ]*/\\1/p'"
at_fn_check_prepare_notrace 'an embedded newline' "autotest.at:839"
( $at_check_trace; $CONFIG_SHELL ./micro-suite -l |
	   sed -n 's/.*[0-9]: [^	 ][^	 ]*[	 ][	 ]*\(.*[^	 ]\)[	 ]*/\1/p'
