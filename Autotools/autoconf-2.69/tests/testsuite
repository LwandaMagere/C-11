#include <math.h>

#ifdef __cplusplus
extern "C"  /* prevent C++ name mangling */
#endif
void @foobar@ (int *x);

/* Taken from autoconf.texi:Fortran Compiler.  */
#ifdef F77_DUMMY_MAIN
#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN () { return 1; }
#endif

int main(int argc, char *argv[])
{
  int x;
  @foobar@ (&x);
  if (x != 42)
    return 1;
  return 0;
}
_ATEOF


