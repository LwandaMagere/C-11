# Using a symlinked cache file works.
: > cache
rm -f config.cache
$as_ln_s cache config.cache
 { set +x
$as_echo "$at_srcdir/base.at:464: ./configure \$configure_options -C"
at_fn_check_prepare_dynamic "./configure $configure_options -C" "base.at:464"
