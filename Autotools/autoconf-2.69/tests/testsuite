# Use *single* quotes because the context is an unquoted here-doc.
AC_CONFIG_COMMANDS([report],
[test -f "$srcdir/configure.ac" ||
   AC_MSG_ERROR([cannot find $srcdir/configure.ac])],
		   [srcdir='$srcdir'])

