_ATEOF



i=1
while test $i != 101; do
  echo "content of file $i" > file_$i
  as_fn_arith $i + 1 && i=$as_val
done


# ------------ #
# configure.ac #
# ------------ #


cat >configure.ac <<'_ATEOF'
m4_define([AC_DUMMY_VAR],
[ac_Dummy_[]m4_bpatsubst([000$1], [.*\(...\)$], [\1])])

m4_define([AC_DESCRIPTION],
[Define to a long string if your `Autoconf' works properly.])

m4_define([AC_BIG_VALUE],
[This value should be long enough to torture the various limits of sed and other tools used by Autoconf.])

# AC_DEFUBST(NAME)
# ----------------
# Related VALUE to NAME both with AC_SUBST and AC_DEFINE.  This is
# used in the torture tests.
m4_defun([AC_DEFUBST],
[AC_DUMMY_VAR($1)="AC_BIG_VALUE"
AC_DEFINE_UNQUOTED(AC_DUMMY_VAR($1), "$AC_DUMMY_VAR($1) $AC_DUMMY_VAR($1)",
		   AC_DESCRIPTION)
AC_SUBST(AC_DUMMY_VAR($1))
AC_SUBST_FILE([f]$1)
f$1=file_$1
])

AC_INIT
AC_CONFIG_HEADERS(config.h:config.hin)
AC_CONFIG_FILES(dummy)
m4_for(AC_Count, 1, 100, 1,
       [AC_DEFUBST(AC_Count)])
AC_PROG_AWK
AC_OUTPUT
_ATEOF
# configure.ac

{ set +x
$as_echo "$at_srcdir/torture.at:718: autoconf --force "
at_fn_check_prepare_trace "torture.at:718"
( $at_check_trace; autoconf --force
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:718"
$at_failed && at_fn_log_failure
$at_traceon; }



if test -s configure && test "$ac_cv_sh_n_works" = yes; then
  $as_echo "torture.at:718" >"$at_check_line_file"
(test "$ac_cv_sh_n_works" != yes) \
  && at_fn_check_skip 77 "$at_srcdir/torture.at:718"
{ set +x
$as_echo "$at_srcdir/torture.at:718: /bin/sh -n configure"
at_fn_check_prepare_trace "torture.at:718"
( $at_check_trace; /bin/sh -n configure
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:718"
$at_failed && at_fn_log_failure
$at_traceon; }

fi

{ set +x
$as_echo "$at_srcdir/torture.at:719: autoheader "
at_fn_check_prepare_trace "torture.at:719"
( $at_check_trace; autoheader
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:719"
$at_failed && at_fn_log_failure
$at_traceon; }


# Check both awk and the result of AC_PROG_AWK.
# Quote the first word in the for list for Solaris sh.
for awk_arg in "FOO=" AWK=awk; do
   { set +x
$as_echo "$at_srcdir/torture.at:723: ./configure \$configure_options \$awk_arg"
at_fn_check_prepare_dynamic "./configure $configure_options $awk_arg" "torture.at:723"
( $at_check_trace; ./configure $configure_options $awk_arg
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/torture.at:723"
$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }


  # Checking that AC_DEFINE worked properly.
  cat >expout <<'_ATEOF'
