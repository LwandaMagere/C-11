#AT_START_378
at_fn_group_banner 378 'semantics.at:646' \
  "AC_PATH_PROGS_FEATURE_CHECK" "                    " 17
at_xfail=no
(
  $as_echo "378. $at_setup_line: testing $at_desc ..."
  $at_traceon


# This test doesn't work if `pwd` contains white space
case `pwd` in
  *\ * | *\	*) { set +x
$as_echo "$at_srcdir/semantics.at:650: exit 77"
at_fn_check_prepare_trace "semantics.at:650"
( $at_check_trace; exit 77
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
