$at_failed && at_fn_log_failure  \
"config.log"
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/torture.at:309: ./config.status --file="
at_fn_check_prepare_trace "torture.at:309"
( $at_check_trace; ./config.status --file=
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
